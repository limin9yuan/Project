<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header">
<link href="/css/layui.css" rel="stylesheet">
</head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<!-- tab标签 -->
						<ul id="myTab" class="nav nav-tabs">
							<li class="active"><a href="#detailed_information" data-toggle="tab">项目信息</a></li>
							<li><a href="#labor_cost" data-toggle="tab">人工成本</a></li>
							<li><a href="#details_of_reimbursement" data-toggle="tab">报销详情</a></li>
							<li><a href="#details_of_procurement" data-toggle="tab">采购详情</a></li>
							<li><a href="#details_of_the_return" data-toggle="tab">回款详情</a></li>
							<li><a href="#summary_of_income_and_expenditure" data-toggle="tab">收支情况汇总</a></li>
						</ul>
<!--项目信息-->
						<form class="form-horizontal m-t" id="signupForm">
							<div id="myTabContent" class="tab-content">
								<div class="tab-pane fade in active" id="detailed_information">
								<input id="paramProjectId" name="paramProjectId" th:value="${projectId}" class="form-control" type="text" type="hidden">
									<div class="form-group">
										<label class="col-sm-2 control-label">客户名称：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="customerId"
												id="customerId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">业务名称：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="businessId"
												id="businessId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目名称：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="projectId"
												id="projectId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">项目开始时间：</label>
										<div class="col-sm-4">
											<div class="input-group date" id="projectBeginDate">
												<input class="form-control" type="text"
													name="projectBeginDate"
													> <span
													class="input-group-addon"> <span
													class="glyphicon glyphicon-calendar"></span>
												</span>
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目结束时间：</label>
										<div class="col-sm-4">
											<div class="input-group date" id="projectEndDate">
												<input class="form-control" type="text"
													name="projectEndDate"> <span
													class="input-group-addon"> <span
													class="glyphicon glyphicon-calendar"></span>
												</span>
											</div>
										</div>
										<label class="col-sm-2 control-label">项目计划利润率：</label>
										<div class="col-sm-4">
											<input id="budgetProfitRate" name="budgetProfitRate" class="form-control" type="text">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目计划利润：</label>
										<div class="col-sm-4">
											<input id="" name="" class="form-control" type="text">
										</div>
										<label class="col-sm-2 control-label">项目实际利润率：</label>
										<div class="col-sm-4">
											<input id="" name="" class="form-control" type="text">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目实际利润：</label>
										<div class="col-sm-4">
											<input id="" name="" class="form-control" type="text">
										</div>
										<label class="col-sm-2 control-label">项目主管：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="projectOwner"
												id="projectOwner" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目经理：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="projectManager"
												id="projectManager" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">参与中心：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name="responsibleCenter"
												id="responsibleCenter" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目预期验收时间：</label>
										<div class="col-sm-4">
											<div class="input-group date" id="contractCommitTime">
												<input class="form-control" type="text"
													name="contractCommitTime"> <span
													class="input-group-addon"> <span
													class="glyphicon glyphicon-calendar"></span>
												</span>
											</div>
										</div>
										<label class="col-sm-2 control-label">项目金额（除税）：</label>
										<div class="col-sm-4">
											<select data-placeholder="请选择" name=""
												id="" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目描述：</label>
										<div class="col-sm-10">
											<textarea id="projectDescription" name="projectDescription"
												class="form-control" rows="3"></textarea>
										</div>
									</div>
								</div>
<!-- 人工成本 -->
								  <div class="tab-pane fade" id="labor_cost">
									<div class="ibox">
										<div class="ibox-body">
											<div class="fixed-table-toolbar"></div>
											<table id="exampleTable" data-mobile-responsive="true"></table>
										</div>
									</div>
									<div>
										<script type="text/javascript">
											var s_edit_h = 'hidden';
											var s_remove_h = 'hidden';
										</script>
									</div>
								</div>  
<!-- 报销信息 -->
								  <div class="tab-pane fade" id="details_of_reimbursement">
									<div class="ibox">
										<div class="ibox-body">
											<div class="fixed-table-toolbar"></div>
											<table id="expensesNormalTable" data-mobile-responsive="true"></table>
										</div>
									</div>
 									<div>
										<script type="text/javascript">
											var s_edit_h = 'hidden';
											var s_remove_h = 'hidden';
										</script>
									</div>
								</div> 
<!-- 采购详情 --> 
								<div class="tab-pane fade" id="details_of_procurement">
									<div class="ibox">
										<div class="ibox-body">
											<div class="fixed-table-toolbar"></div>
											<table id="purchaseTable" data-mobile-responsive="true"></table>
										</div>
									</div>
 									<div>
										<script type="text/javascript">
											var s_edit_h = 'hidden';
											var s_remove_h = 'hidden';
										</script>
									</div>
								</div>
<!-- 回款信息 -->					
								<div class="tab-pane fade" id="details_of_the_return">
									<div class="ibox">
										<div class="ibox-body">
											<div class="fixed-table-toolbar"></div>
											<table id="receivedTable" data-mobile-responsive="true"></table>
										</div>
									</div>
 									<div>
										<script type="text/javascript">
											var s_edit_h = 'hidden';
											var s_remove_h = 'hidden';
										</script>
									</div>
								</div>
<!-- 收支详情总汇 -->
								<div class="tab-pane fade" id="summary_of_income_and_expenditure">
									<div class="ibox">
										<div class="ibox-body">
											<div class="fixed-table-toolbar"></div>
											<table id="summaryOfIncomeAndExpenditureTable" data-mobile-responsive="true"></table>
										</div>
									</div>
 									<div>
										<script type="text/javascript">
											var s_edit_h = 'hidden';
											var s_remove_h = 'hidden';
										</script>
									</div>
								</div>

							</div>
						</form>
					</div>
			</div>
		</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script src="/js/layui.js"></script>
	<script src="/js/vue.min.js"></script>
	<script src="/js/plugins/layer/layer.min.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/projectExpenditure/detailed_information.js"></script>
	<script type="text/javascript" src="/js/appjs/budget/labor/labor.js"></script>
	<script type="text/javascript" src="/js/appjs/approval/expensesNormal/expensesNormal.js"></script>
	<script type="text/javascript" src="/js/appjs/approval/purchase/purchase.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/received/received.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/projectExpenditure/summaryOfIncomeAndExpenditure.js"></script>
</body>
</html>
