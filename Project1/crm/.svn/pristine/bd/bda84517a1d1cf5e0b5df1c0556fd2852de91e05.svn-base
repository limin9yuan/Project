<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header">
<link href="/css/layui.css" rel="stylesheet">
</head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="recordId" name="recordId" th:value="${record.recordId}"  type="hidden">
						<div class="form-group">
								<label class="col-sm-3 control-label">客户名称：</label>
								<div class="col-sm-9">
									<select data-placeholder="请选择" name="customerId" id="customerId"
									th:value="${record.customerId}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>	
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">业务名称：</label>
								<div class="col-sm-9">
									<select data-placeholder="请选择" name="businessId" id="businessId"
									th:value="${record.businessId}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>	
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目名称：</label>
								<div class="col-sm-9">
									<select data-placeholder="请选择" name="projectId" id="projectId"
									th:value="${record.projectId}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>	
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">行动主题：</label>
								<div class="col-sm-3">
									<input id="recordName" name="recordName" th:value="${record.recordName}" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">阶段：</label>
								<div class="col-sm-3">
									<select data-placeholder="请选择" name="recordPhase" id="recordPhase"
									th:value="${record.recordPhase}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">执行人：</label>
								<div class="col-sm-3">
									<select data-placeholder="请选择" name="recordExecutor" id="recordExecutor"
									th:value="${record.recordExecutor}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>
								<label class="col-sm-3 control-label">执行状态：</label>
								<div class="col-sm-3">
									<select data-placeholder="请选择" name="recordExecuteStatus" id="recordExecuteStatus"
									th:value="${record.recordExecuteStatus}"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	
									</select>
								</div>
							</div>
							<div class="form-group">
							<label class="col-sm-3 control-label">实际执行开始时间：</label>
								<div class="col-sm-3">
									<div class="input-group date" id="recordBeginDate">
							            <input th:value="${record.recordBeginDate}" class="form-control" type="text" name="recordBeginDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							<label class="col-sm-3 control-label">实际执行结束时间：</label>
								<div class="col-sm-3">
									<div class="input-group date" id="recordEndDate">
							            <input th:value="${record.recordEndDate}" class="form-control" type="text" name="recordEndDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>	
							</div>
									
							<div class="form-group">	
								<label class="col-sm-3 control-label">销售机会：</label>
								<div class="col-sm-3">
									<input id="recordSaleOpportunity" name="recordSaleOpportunity" th:value="${record.recordSaleOpportunity}" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">实际费用：</label>
								<div class="col-sm-3">
									<input id="recordExpenseActual" name="recordExpenseActual" th:value="${record.recordExpenseActual}" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">费用类型：</label>
								<div class="col-sm-3">
									<select data-placeholder="请选择" name="recordExpenseCategory" id="recordExpenseCategory"
										th:value="${record.recordExpenseCategory}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>
							</div>
							<div class="form-group">
							   <label class="col-sm-3 control-label">附件：</label>
							   <div class="col-sm-3">
							        <div class="file-manager">
								       <button type="button" class="layui-btn" id="test1" style="width: 100%;">
									       <i class="fa fa-cloud"></i>上传文件
								       </button>
									   <input id="recordAttachment" name="recordAttachment" type="hidden">
							        </div>
						        </div>			
							</div>
						<!-- 	<div class="form-group">	
								<label class="col-sm-3 control-label">附件：</label>
								<div class="col-sm-3">
									<input id="recordAttachment" name="recordAttachment" th:value="${record.recordAttachment}" class="form-control" type="text">
								</div>
								<div class="col-sm-6">
									<h6>文件可用拓展名：pdf、jpg</h6>
								</div>
							</div> -->
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								<textarea id="recordRemarks" name="recordRemarks" th:value="${record.recordRemarks}" class="form-control" rows="5"></textarea>								
								</div>
							</div>
							<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							</div>	
							
