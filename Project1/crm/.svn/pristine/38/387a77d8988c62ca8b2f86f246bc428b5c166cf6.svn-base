<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header">
<link href="/css/layui.css" rel="stylesheet">
</head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<!--  <input id="invoiceId" name="invoiceId" th:value="${invoice.invoiceId}"  type="hidden">-->
						<div class="form-group">	
								<label class="col-sm-3 control-label">发票序号：</label>
								<div class="col-sm-3">
									<input id="invoiceId" name="invoiceId" th:value="${invoice.invoiceId}" class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">开票公司：</label>
								<div class="col-sm-3">
									<input id="invoiceCompany" name="invoiceCompany" th:value="${invoice.invoiceCompany}" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">发票号码：</label>
								<div class="col-sm-3">
									<input id="invoiceNumber" name="invoiceNumber" th:value="${invoice.invoiceNumber}" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">发票金额：</label>
								<div class="col-sm-3">
									<input id="invoicePrice" name="invoicePrice" th:value="${invoice.invoicePrice}" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">发票类型：</label>
								<div class="col-sm-3">
									<input id="invoiceType" name="invoiceType" th:value="${invoice.invoiceType}" class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">发票内容：</label>
								<div class="col-sm-3">
									<input id="invoiceContent" name="invoiceContent" th:value="${invoice.invoiceContent}" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">开票人：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="invoicePerson" id="invoicePerson"
										th:value="${invoice.invoicePerson}"  class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option> 
									</select>
								</div>	
								<label class="col-sm-3 control-label">发票领取人：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="invoiceReceiver" id="invoiceReceiver"
										th:value="${invoice.invoiceReceiver}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option> 
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">开票日期：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="invoiceDate">
							            <input th:value="${invoice.invoiceDate}" class="form-control" type="text" name="invoiceDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>	
								<label class="col-sm-3 control-label">领取日期：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="invoiceReceiverTime">
							            <input th:value="${invoice.invoiceReceiverTime}" class="form-control" type="text" name="invoiceReceiverTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">合同状态：</label>
								<div class="col-sm-9">
								    <label th:value="${invoice.invoiceContractStatus}" class="radio-inline"> 
									    <input type="radio" name="invoiceContractStatus" value="0" /> 未开始
									</label> 
									<label th:value="${invoice.invoiceContractStatus}" class="radio-inline"> 
									    <input type="radio" name="invoiceContractStatus" value="1" /> 交付中
									</label>
									<label th:value="${invoice.invoiceContractStatus}" class="radio-inline"> 
									    <input type="radio" name="invoiceContractStatus" value="2" /> 已完成
									</label>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">附件：</label>
								<div class="col-sm-3">
									<div class="file-manager">
										<button type="button" class="layui-btn" id="test1"
											style="width: 100%;">
											<i class="fa fa-cloud"></i>上传文件
										</button>
										<input id="invoiceAttachment" name="invoiceAttachment"
											type="hidden">
									</div>
								</div>
							</div>											
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
									<textarea id="invoiceRemarks" name="invoiceRemarks" 
									th:value="${invoice.invoiceRemarks}" class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							</div>
							
							<!--
							<div class="form-group">	
								<label class="col-sm-3 control-label">合同编号：</label>
								<div class="col-sm-8">
									<input id="contractId" name="contractId" th:value="${invoice.contractId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">业务编号：</label>
								<div class="col-sm-8">
									<input id="businessId" name="businessId" th:value="${invoice.businessId}" class="form-control" type="text">
								</div>
							</div> 
							<div class="form-group">	
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="invoiceOperator" name="invoiceOperator" th:value="${invoice.invoiceOperator}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作时间：</label>
								<div class="col-sm-8">
									<input id="invoiceOperateTime" name="invoiceOperateTime" th:value="${invoice.invoiceOperateTime}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>  -->
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script src="/js/layui.js"></script>
	<script src="/js/vue.min.js"></script>
	<script src="/js/plugins/layer/layer.min.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/invoice/edit.js">
	</script>
</body>
</html>
