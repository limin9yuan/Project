<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="purchaseId" name="purchaseId" th:value="${purchaseId}" class="form-control" type="hidden">
						<div class="form-group">	
								<label class="col-sm-3 control-label">采购类型：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="purchaseType" id="purchaseType"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									</select>
								</div>	
								<label class="col-sm-3 control-label">采购金额预估：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchasePrice" name="purchasePrice" 
									 class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">预估说明：</label>
								<div class="col-sm-3">
									<input id="purchaseDescription" name="purchaseDescription" 
									 class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">客户承担：</label>
								<div class="col-sm-3">
									<input id="purchaseCustomerRate" name="purchaseCustomerRate" 
									 class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">项目组承担：</label>
								<div class="col-sm-3">
									<input id="purchaseProjectRate" name="purchaseProjectRate" 
									 class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">总计：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseTotalPrice" name="purchaseTotalPrice" 
									 class="form-control" type="text" readonly>
								</div>
							</div>
							
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								    <textarea id="purchaseRemarks" name="purchaseRemarks" 
								     class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">	
								   <div class="columns pull-right col-md-2">
										<button type="button" class="btn btn-danger" onclick="closeWin()">取消</button>
								   </div>
								   <div class="columns pull-right col-md-2">
										<button type="submit" class="btn btn-primary columns" >提交</button>
								  </div>
							 </div>
							    
