<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
	<div class="row">
		  <div class="col-sm-3">
				<div class="ibox ibox-body">
					<div class="ibox-title">
						<h5>海心信息</h5>
					</div>
					<div class="ibox-content">
						<div id="jstree"></div>
					</div>
				</div>
		 </div>
		<div class="col-sm-9">
			<div class="ibox">
				<div class="ibox-body">
						<form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">
								<label class="col-sm-2 control-label">员工姓名：</label>
								<div class="col-sm-4">
									<select data-placeholder="全部" name="employeeName"
										id="employeeName" class="form-control chosen-select"
										tabindex="2" style="width: 100%">
										<option value="">全部</option>
									</select>
								</div>
								<label class="col-sm-2 control-label">员工编号：</label>
								<div class="col-sm-4">
									<input id="employeeId" name="employeeId" class="form-control"
										type="text">
								</div>
							</div>
							<div class="form-group">
							<label class="col-sm-2 control-label">所属部门：</label>
								<div class="col-sm-4">
									<input id="deptId" name="deptId" class="form-control"
										type="text">
								</div>
								<label class="col-sm-2 control-label">操作人：</label>
								<div class="col-sm-4">
									<input id="employeeOperator" name="employeeOperator"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								 <label class="col-sm-9 control-label"></label> 
								<div class="col-sm-3">
									<button type="button" class="btn btn-danger" onclick="reLoad()">查询</button>
									<button type="button" class="btn btn-danger" onclick="resetPwd()">重置</button>
								</div>
							</div>
							<!-- <div class="text-right form-group">
								<div class="columns pull-right col-md-2">
									<button type="button" class="btn btn-danger" onclick="reLoad()">查询</button>
									<button type="button" class="btn btn-danger" onclick="resetPwd()">重置</button>
								</div>
							</div> -->
						</form>
						<div class="fixed-table-toolbar">
						<div class="columns pull-left">
							<button shiro:hasPermission="inner:innerOrgEmployee:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>添加
							</button>
							<button shiro:hasPermission="inner:innerOrgEmployee:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>批量删除
							</button>
							<button shiro:hasPermission="sales:salesProject:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>导入
							</button>
							<button shiro:hasPermission="sales:salesProject:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>导出
							</button>
							<a shiro:hasPermission="sales:salesProject:batchRemove" type="button" class="btn  btn-danger"
								onclick="returnList()">
								<i class="fa" aria-hidden="true"></i>列表显示项配置
							</a>
						</div>
						<div class="columns pull-right">
							<button class="btn btn-success" onclick="reLoad()">查询</button>
						</div>
						<div class="columns pull-right col-md-2 nopadding">
							<input id="searchName" type="text" class="form-control"
								placeholder="">
						</div>
					</div>
					<table id="exampleTable" data-mobile-responsive="true">
					</table>
				</div>
			</div>
		</div>
	</div>
	<!--shiro控制bootstraptable行内按钮看见性 来自bootdo的创新方案 -->
		<div>
			<script type="text/javascript">
				var s_edit_h = 'hidden';
				var s_remove_h = 'hidden';
				var s_resetPwd_h = 'hidden';
			</script>
		</div>
		<div shiro:hasPermission="inner:innerOrgEmployee:edit">
			<script type="text/javascript">
				s_edit_h = '';
			</script>
		</div>
		<div shiro:hasPermission="inner:innerOrgEmployee:remove">
			<script type="text/javascript">
				var s_remove_h = '';
			</script>
		</div>
		<div shiro:hasPermission="inner:innerOrgEmployee:resetPwd">
			<script type="text/javascript">
				var s_resetPwd_h = '';
			</script>
		</div>
	</div>
	<div th:include="include :: footer"></div>
	<script type="text/javascript" src="/js/appjs/inner/innerOrgEmployee/innerOrgEmployee.js"></script>
</body>
</html>