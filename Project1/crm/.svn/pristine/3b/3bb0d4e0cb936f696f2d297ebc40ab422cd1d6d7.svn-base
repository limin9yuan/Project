		</set>
		where Budget_ID = #{budgetId}
	</update>
	
	<delete id="remove">
		delete from project_budget where Budget_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from project_budget where Budget_ID in 
		<foreach item="budgetId" collection="array" open="(" separator="," close=")">
			#{budgetId}
		</foreach>
	</delete>

