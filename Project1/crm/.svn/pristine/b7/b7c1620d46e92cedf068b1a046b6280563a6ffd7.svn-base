									<input id="contactExperience" name="contactExperience" th:value="${customerContact.contactExperience}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">曾供职单位：</label>
								<div class="col-sm-8">
									<input id="contactPreviousCompany" name="contactPreviousCompany" th:value="${customerContact.contactPreviousCompany}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
