<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="contactId" name="contactId" th:value="${customerContact.contactId}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">姓名：</label>
								<div class="col-sm-8">
									<input id="contactName" name="contactName" th:value="${customerContact.contactName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">性别：</label>
								<div class="col-sm-8">
									<input id="contactSex" name="contactSex" th:value="${customerContact.contactSex}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">称谓：</label>
								<div class="col-sm-8">
									<input id="contactSalutation" name="contactSalutation" th:value="${customerContact.contactSalutation}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
