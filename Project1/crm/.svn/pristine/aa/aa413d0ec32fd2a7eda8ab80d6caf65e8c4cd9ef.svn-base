<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<link href="/css/layui.css" rel="stylesheet">
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
	<form id="additionalRecordsform"
			th:action="@{/contract/additionalRecords/export}" method="get">
			<!--  <input type="hidden" id="province" name="province" /> 
			<input type="hidden" id="city" name="city" /> 
			<input type="hidden" id="area" name="area" /> -->
			<input type="hidden" id="projectId" name="projectId" /> <input
				type="hidden" id="projectSales" name="projectSales" />
		</form>
	<div class="row">
		<div class="col-sm-12">
				<div class="ibox">
				<div class="ibox-body">
				    <form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">
							   <label class="col-sm-1 control-label">客户名称：</label>
							   <div class="col-sm-2">
								<select data-placeholder="全部" name="customerId" id="customerId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">全部</option>
								</select>
								</div>
								<label class="col-sm-1 control-label">业务名称：</label>
								<div class="col-sm-2">
								<select data-placeholder="全部" name="businessId" id="businessId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">全部</option>
								</select>
								</div>
								<label class="col-sm-1 control-label">项目名称：</label>
								<div class="col-sm-2">
								<select data-placeholder="全部" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">全部</option>
									</select>
								</div>
							</div>
							<div class="text-right form-group">
								<div class="columns pull-right col-md-2">
									<button type="button" class="btn btn-danger" onclick="reLoad()">查询</button>
									<button type="button" class="btn btn-danger"
										onclick="resetPwd()">重置</button>
								</div>
							</div>
						</form>
					<div class="fixed-table-toolbar">
						<div class="columns pull-left">
							<button shiro:hasPermission="contract:additionalRecords:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>添加
							</button>
							<button shiro:hasPermission="contract:additionalRecords:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>删除
							</button>
							<button shiro:hasPermission="contract:additionalRecords:uploadExcel"
							type="button" class="btn  btn-primary" name="excelinsertbtn">
							<i class="fa warning" aria-hidden="true"></i>导入
						    </button>
							<button shiro:hasPermission="contract:additionalRecords:export"
									type="button" class="btn  btn-danger" onclick="exportData()">
									<i class="fa" aria-hidden="true"></i>导出
							</button>
							<a shiro:hasPermission="contract:additionalRecords:batchRemove"
								type="button" class="btn  btn-danger" onclick="returnList()">
								<i class="fa" aria-hidden="true"></i>列表显示项配置
							</a>	
						</div>	
						<div class="columns pull-right">
							    <button class="btn btn-danger" onclick="reLoad()">所有合同</button>
						</div>
						<div class="columns pull-right col-md-1 nopadding">
							    <button class="btn btn-danger" onclick="reLoad()">我的合同</button>
						</div>		
					</div>
					<table id="exampleTable" data-mobile-responsive="true">
					</table>
				</div>
				</div>
			</div>
		</div>
	</div>
	
	<!-- 导入按钮弹出框 -->
	<div name="additionalRecords_windows" class="modal fade"
		tabindex="-1" data-width="50%">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
			<h4 class="modal-title">合同增补信息导入</h4>
		</div>
		<div class="modal-body">
			<div class="row">
				<div class="form-group">
										<!-- <label class="col-sm-2 control-label">附件：</label> -->
										<div class="col-sm-4">
											<div class="file-manager">
												<button type="button" class="layui-btn" id="FileTest1"
													style="width: 100%;">
													<i class="fa fa-cloud"></i>上传文件
												</button>
												<input id="additionalRecordsFile" name="additionalRecordsFile" type="hidden">
											</div>
										</div>
									</div>
			</div>
			<div class="modal-footer">
		<button type="button" data-dismiss="" class="btn blue btn-sm"
			name="tab_excelinsertOkbtn">
			<i class="glyphicon glyphicon-saved"></i>&nbsp;导入
		</button>
		<button type="button" class="btn default btn-sm" data-dismiss="modal" 
		   name="tab_excelinsertQuitbtn">
			<i class="glyphicon glyphicon-remove"></i>&nbsp;取消
		</button>
	</div>
		</div>
	</div>
	<!--shiro控制bootstraptable行内按钮看见性 来自bootdo的创新方案 -->
		<div>
			<script type="text/javascript">
				var s_edit_h = 'hidden';
				var s_remove_h = 'hidden';
				var s_resetPwd_h = 'hidden';
			</script>
		</div>
		<div shiro:hasPermission="contract:additionalRecords:edit">
			<script type="text/javascript">
				s_edit_h = '';
			</script>
		</div>
		<div shiro:hasPermission="contract:additionalRecords:remove">
			<script type="text/javascript">
				var s_remove_h = '';
			</script>
		</div>
		<div shiro:hasPermission="contract:additionalRecords:resetPwd">
			<script type="text/javascript">
				var s_resetPwd_h = '';
			</script>
		</div>
	</div>
	<div th:include="include :: footer"></div>
	<script src="/js/layui.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/additionalRecords/additionalRecords.js"></script>
</body>
</html>