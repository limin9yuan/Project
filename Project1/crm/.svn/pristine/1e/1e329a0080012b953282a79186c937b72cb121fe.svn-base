	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="productId" name="productId" th:value="${productCategory.productId}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">产品名称：</label>
								<div class="col-sm-3">
									<input id="productName" name="productName" th:value="${productCategory.productName}" 
									class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">产品描述：</label>
								<div class="col-sm-9">
								    <textarea id="productDescription" name="productDescription" th:value="${productCategory.productDescription}"
								    class="form-control" rows="3"></textarea>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">附件：</label>
								<div class="col-sm-3">
									<div class="file-manager">
										<button type="button" class="layui-btn" id="test1"
											style="width: 100%;">
											<i class="fa fa-cloud"></i>上传文件
										</button>
										<input id="productAttachment" name="productAttachment"
											type="hidden">
									</div>
								</div>
							</div>	
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								<textarea id="productRemars" name="productRemars" th:value="${productCategory.productRemars}" 
								class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							</div>
	
