<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="paidId" name="paidId" th:value="${paid.paidId}"  type="hidden">
						<div class="form-group">	
								<label class="col-sm-3 control-label">付款金额：</label>
								<div class="col-sm-3">
									<input id="paidPrice" name="paidPrice" th:value="${paid.paidPrice}" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">应付款金额：</label>
								<div class="col-sm-3">
									<input id="paidAccountPrice" name="paidAccountPrice" th:value="${paid.paidAccountPrice}" class="form-control" type="text">
								</div>
						</div>
						<div class="form-group">	
								<label class="col-sm-3 control-label">付款账号：</label>
								<div class="col-sm-3">
									<input id="paidAccountNumber" name="paidAccountNumber" th:value="${paid.paidAccountNumber}" class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">款项类型：</label>
								<div class="col-sm-3">
									<input id="paidType" name="paidType" th:value="${paid.paidType}"  class="form-control" type="text">
								</div>
						</div>	
						<div class="form-group">	
								<label class="col-sm-3 control-label">付款时间：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="paidTime">
							            <input th:value="${paid.paidTime}" class="form-control" type="text" name="paidTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							<label class="col-sm-3 control-label">收款延迟时间：</label>
								<div class="col-sm-3">
									<input id="receiptDelayTime" name="receiptDelayTime" th:value="${paid.receiptDelayTime}" class="form-control" type="text">
								</div>
						</div>							
						<div class="form-group">	
								<label class="col-sm-3 control-label">合同状态：</label>
								<div class="col-sm-9">
									<label th:value="${paid.paidStatus}" class="radio-inline"> 
									    <input type="radio" name="paidStatus" value="0" /> 未开始
									</label> 
									<label th:value="${paid.paidStatus}" class="radio-inline"> 
									    <input type="radio" name="paidStatus" value="1" /> 交付中
									</label>
									<label th:value="${paid.paidStatus}" class="radio-inline"> 
									    <input type="radio" name="paidStatus" value="2" /> 已完成
									</label>
								</div>
						</div>
						<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
									<textarea id="paidRemarks" name="paidRemarks" th:value="${paid.paidRemarks}" class="form-control" rows="5"></textarea>
								</div>
						</div>
						<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							</div>
							
