	<delete id="remove">
		delete from sales_online_feedback where FeedBack_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from sales_online_feedback where FeedBack_ID in 
		<foreach item="feedbackId" collection="array" open="(" separator="," close=")">
			#{feedbackId}
		</foreach>
	</delete>

