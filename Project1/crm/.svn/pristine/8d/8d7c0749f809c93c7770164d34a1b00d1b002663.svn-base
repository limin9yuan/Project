<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<input id="receivedId" name="receivedId" class="form-control" type="hidden" th:value="${receivedId}"> 
							<div class="form-group">
								<label class="col-sm-2 control-label">收款时间：：</label>
								<div class="col-sm-4">
									<div class="input-group date" id="receivedTime">
										<input class="form-control" type="text" name="receivedTime">
										<span class="input-group-addon"> <span
											class="glyphicon glyphicon-calendar"></span>
										</span>
									</div>
								</div>
								<label class="col-sm-2 control-label">收款金额：</label>
								<div class="col-sm-4">
									<input id="receivedPrice" name="receivedPrice"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">收款账户：</label>
								<div class="col-sm-4">
									<input id="receivedCardNumber" name="receivedCardNumber"
										class="form-control" type="text">
								</div>
								<label class="col-sm-2 control-label">款项类型：</label>
								<div class="col-sm-4">
									<input id="receivedType" name="receivedType"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">合同状态：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="receivedContractStatus"
										id=receivedContractStatus class="form-control chosen-select"
										tabindex="2" style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">备注：</label>
								<div class="col-sm-10">
									<textarea id="receivedRemarks" name="receivedRemarks"
										class="form-control" rows="3"></textarea>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-3 col-sm-offset-9">
									<button type="submit" class="btn btn-primary">提交</button>
									&nbsp;
									<button type="button" class="btn btn-primary" onclick="closeWin()">取消</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/payment/received/edit.js"></script>
	
</body>
</html>
