<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="col-sm-12">
			<div class="ibox float-e-margins">
				<div class="ibox-content">
				<form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">
							    <label class="col-sm-1 control-label">日期：</label>
								<div class="col-sm-2">
									<div class="input-group date" id="timesheetBeginDate">
							            <input class="form-control" data-placeholder="开始日期" type="text" name="timesheetBeginDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>	
								<div class="col-sm-2">
							        <div class="input-group date" id="timesheetEndDate">
							            <input class="form-control" data-placeholder="结束日期" type="text" name="timesheetEndDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
							    </div>
							    <div class="col-sm-2">
							    <button type="button"
							        class="btn btn-danger" onclick="reLoad()">本周</button>
							    <button type="button"
						            class="btn btn-danger" onclick="resetPwd()">上周</button>
						        
						        </div>			
								<label class="col-sm-1 control-label">项目名称：</label>
								<div class="col-sm-2">
								<select data-placeholder="全部" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">全部</option>
									</select>
								</div>
							</div>
							<div class="text-right form-group">
							<div class="columns pull-right col-md-2">
							    <button type="button"
							        class="btn btn-danger" onclick="reLoad()">查询</button>
							    <button type="button"
						            class="btn btn-danger" onclick="resetPwd()">重置</button>
						        
						    </div>
							</div>
						</form>
					<div class="fixed-table-toolbar">
						<div class="columns pull-left">
							<button shiro:hasPermission="timesheet:timesheet:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>添加
							</button>
							<button shiro:hasPermission="timesheet:timesheet:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>删除
							</button>
						</div>
						<div class="columns pull-right">
							<button class="btn btn-success" onclick="reLoad()">查询</button>
						</div>
						<div class="columns pull-right col-md-2 nopadding">
							<input id="searchName" type="text" class="form-control"
								placeholder="">
						</div>
					</div>
					<table id="exampleTable" data-mobile-responsive="true">
					</table>
				</div>
			</div>
		</div>
	</div>
	<!--shiro控制bootstraptable行内按钮看见性 来自bootdo的创新方案 -->
		<div>
			<script type="text/javascript">
				var s_edit_h = 'hidden';
				var s_remove_h = 'hidden';
				var s_resetPwd_h = 'hidden';
			</script>
		</div>
		<div shiro:hasPermission="timesheet:timesheet:edit">
			<script type="text/javascript">
				s_edit_h = '';
			</script>
		</div>
		<div shiro:hasPermission="timesheet:timesheet:remove">
			<script type="text/javascript">
				var s_remove_h = '';
			</script>
		</div>
		<div shiro:hasPermission="timesheet:timesheet:resetPwd">
			<script type="text/javascript">
				var s_resetPwd_h = '';
			</script>
		</div>
	</div>
	<div th:include="include :: footer"></div>
	<script type="text/javascript" src="/js/appjs/timesheet/timesheet/timesheet.js"></script>
</body>
</html>