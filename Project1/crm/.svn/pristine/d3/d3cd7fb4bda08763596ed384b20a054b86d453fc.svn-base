<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<!--  <input id="assignmentId" name="assignmentId" th:value="${assignment.assignmentId}"  type="hidden">-->
						<input id="assignment" name="assignment" th:value="${assignmentId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${customerId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${businessId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${projectId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentProjectCagegory}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentPm}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentPerson}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${deptId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${jobId}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentTaskName}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentTaskPerformance}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentBeginTime}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentEndTime}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentDept}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentPrincipal}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentRecipientDept}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentRecipient}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentTaskDescription}" class="form-control" type="text" type="hidden">
						<input id="assignment" name="assignment" th:value="${assignmentRemarks}" class="form-control" type="text" type="hidden">
						<div class="form-group">	
								<label class="col-sm-3 control-label">客户名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="customerId" id="customerId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
								<label class="col-sm-3 control-label">业务名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="businessId" id="businessId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">项目名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>	
								<label class="col-sm-3 control-label">项目类别：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentProjectCagegory" id="assignmentProjectCagegory"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">项目经理：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentPm" id="assignmentPm"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
