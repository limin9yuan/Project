<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="budgetId" name="budgetId" th:value="${budget.budgetId}"  type="hidden">
						    <div class="form-group">	
									<label class="col-sm-3 control-label">负责中心：</label>
									<div class="col-sm-3">
								    <select data-placeholder="请选择" name="responsibleCenter" id="responsibleCenter"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
									<label class="col-sm-3 control-label">部门：</label>
									<div class="col-sm-3">
						                 <select data-placeholder="请选择" name="deptId" id="deptId"
										th:value="${budget.deptId}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">项目主管：</label>
									<div class="col-sm-3">
									    <select data-placeholder="请选择" name="projectSupervisor" id="projectSupervisor"
										th:value="${budget.projectSupervisor}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
									<label class="col-sm-3 control-label">项目经理：</label>
									<div class="col-sm-3">
									    <select data-placeholder="请选择" name="projectOwner" id="projectOwner"
										th:value="${budget.projectOwner}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">项目名称：</label>
									<div class="col-sm-3">
									    <select data-placeholder="请选择" name="projectId" id="projectId"
										th:value="${budget.projectId}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
									<label class="col-sm-3 control-label">项目类别：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectGategory" id="projectGategory"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
								</div>
							    <div class="form-group">	
									<label class="col-sm-3 control-label">客户名称：</label>
									<div class="col-sm-3">
									    <select data-placeholder="请选择" name="customerId" id="customerId"
										th:value="${budget.customerId}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
									<label class="col-sm-3 control-label">业务名称：</label>
									<div class="col-sm-3">
									    <select data-placeholder="请选择" name="businessId" id="businessId"
										th:value="${budget.businessId}" class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
									</div>
								</div>
								<div class="form-group">	
								<label class="col-sm-3 control-label">合同名称：</label>
										<div class="col-sm-3">
											<select data-placeholder="请选择" name="contractId" id="contractId" 
											th:value="${budget.contractId}" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
							    </div>		
								<div class="form-group">	
									<label class="col-sm-3 control-label">应收账款总额：</label>
									<div class="col-sm-3">
										<input id="budgetAccountReceivable" name="budgetAccountReceivable" class="form-control" type="text">
									</div>
									<label class="col-sm-3 control-label">计划成本总额：</label>
									<div class="col-sm-3">
										<input id="budgetTotalCost" name="budgetTotalCost" class="form-control" type="text">
									</div>
								</div>							
								<div class="form-group">	
									<label class="col-sm-3 control-label">项目计划利润率：</label>
									<div class="col-sm-3">
										<input id="budgetProfitRate" name="budgetProfitRate" th:value="${budget.budgetProfitRate}" class="form-control" type="text">
									</div>	
									<label class="col-sm-3 control-label">计划是否合规：</label>
									<div class="col-sm-3">
										<input id="budgetConformance" name="budgetConformance" th:value="${budget.budgetConformance}" class="form-control" type="text">
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">服务收入（合同额）：</label>
									<div class="col-sm-3">
										<input id="budgetServiceRevenue" name="budgetServiceRevenue" th:value="${budget.budgetServiceRevenue}" class="form-control" type="text">
									</div>
									<label class="col-sm-3 control-label">税金：</label>
									<div class="col-sm-3">
										<input id="budgetTax" name="budgetTax" th:value="${budget.budgetTax}" class="form-control" type="text">
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">服务净收入：</label>
									<div class="col-sm-3">
										<input id="budgetServiceRevenueNet" name="budgetServiceRevenueNet" th:value="${budget.budgetServiceRevenueNet}" class="form-control" type="text">
									</div>	
									<label class="col-sm-3 control-label">费用和支持（含税）：</label>
									<div class="col-sm-3">
										<input id="budgetCost" name="budgetCost" th:value="${budget.budgetCost}" class="form-control" type="text">
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">采购成本：</label>
									<div class="col-sm-3">
										<input id="budgetPurchaseCost" name="budgetPurchaseCost" th:value="${budget.budgetPurchaseCost}" class="form-control" type="text">
									</div>	
									<label class="col-sm-3 control-label">人工成本：</label>
									<div class="col-sm-3">
										<input id="budgetLaborCost" name="budgetLaborCost" th:value="${budget.budgetLaborCost}" class="form-control" type="text">
									</div>
								</div>
								<div class="form-group">	
									<label class="col-sm-3 control-label">差旅成本：</label>
									<div class="col-sm-3">
										<input id="budgetTravelCost" name="budgetTravelCost" th:value="${budget.budgetTravelCost}" class="form-control" type="text">
									</div>	
									<label class="col-sm-3 control-label">利润：</label>
									<div class="col-sm-3">
										<input id="budgetProfit" name="budgetProfit" th:value="${budget.budgetProfit}" class="form-control" type="text">
									</div>
								</div>
								<div class="form-group">
								<label class="col-sm-3 control-label">项目行政区划：</label>
										 <div class="col-sm-9">
											<select data-placeholder="省" name="province" id="province"
												class="form-control chosen-select" tabindex="1"
												style="width:100px">
											</select> 
											<select data-placeholder="市" name="city" id="city"
												class="form-control chosen-select" tabindex="2"
												style="width:100px">
											</select>
											<select data-placeholder="区" name="area" id="area"
												class="form-control chosen-select" tabindex="3"
												style="width:100px">
											</select>
									    </div>
								</div>		
								<div class="form-group">	
									<label class="col-sm-3 control-label">项目描述：</label>
									<div class="col-sm-9">
									    <textarea id="projectDescription" name="projectDescription" class="form-control" rows="3"></textarea>
									</div>
							    </div>
							    <div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							    </div>	
							    
							    
							
														<!-- 
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="budgetOperator" name="budgetOperator" th:value="${budget.budgetOperator}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作时间：</label>
								<div class="col-sm-8">
									<input id="budgetOperateTime" name="budgetOperateTime" th:value="${budget.budgetOperateTime}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div> -->
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/budget/budget/edit.js">
	</script>
</body>
</html>
