<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="col-sm-12">
		<div class="ibox float-e-margins">
					<div class="ibox-content">
					<form class="form-horizontal m-t" id="signupForm">
						<div class="form-group">
							<label class="col-sm-2 control-label">行政区划：</label>
							<div class="col-sm-2">
								<select data-placeholder="全部" name="provice" id="provice"
									class="form-control chosen-select data-max-level=3"
									tabindex="2" style="width: 100%">
									<option value="">省</option>
								</select>
								<select data-placeholder="全部" name="provice" id="provice"
									class="form-control chosen-select data-max-level=3"
									tabindex="2" style="width: 100%">
									<option value="">市</option>
								</select>
								<select data-placeholder="全部" name="provice" id="provice"
									class="form-control chosen-select data-max-level=3"
									tabindex="2" style="width: 100%">
									<option value="">区</option>
								</select>
							</div>
							<label class="col-sm-2 control-label">业务名称：</label>
							<div class="col-sm-2">
								<select data-placeholder="全部" name="businessName" id="businessName"
									class="form-control chosen-select" tabindex="2"
									style="width: 100%">
									<option value="">全部</option>
								</select>
							</div>
							<label class="col-sm-2 control-label">销售负责人：</label>
							<div class="col-sm-2">
								<select data-placeholder="全部" name="businessSales"
									id="businessSales" class="form-control chosen-select"
									tabindex="2" style="width: 100%">
									<option value="">全部</option>
								</select>
							</div>
						</div>
						<div class="text-right form-group">
							<div class="columns pull-right col-md-2">
								<button type="button" class="btn btn-danger" onclick="reLoad()">查询</button>
								<button type="button" class="btn btn-danger"
									onclick="resetPwd()">重置</button>
							</div>
						</div>
					</form>
					<div class="fixed-table-toolbar">
						<div class="columns pull-left">
							<button shiro:hasPermission="sales:salesProject:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>新增
							</button>
							<button shiro:hasPermission="sales:salesProject:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>批量删除
							</button>
							<button shiro:hasPermission="sales:salesProject:add" type="button" class="btn  btn-primary" onclick="add()">
								<i class="fa fa-plus" aria-hidden="true"></i>导入
							</button>
							<button shiro:hasPermission="sales:salesProject:batchRemove" type="button" class="btn  btn-danger"
								onclick="batchRemove()">
								<i class="fa fa-trash" aria-hidden="true"></i>导出
							</button>
							<a shiro:hasPermission="sales:salesProject:batchRemove" type="button" class="btn  btn-danger"
								onclick="returnList()">
								<i class="fa" aria-hidden="true"></i>列表显示项配置
							</a>
						</div>
					</div>
					<table id="exampleTable" data-mobile-responsive="true">
					</table>
				</div>
			</div>
		
		
		
		
		
		
		
