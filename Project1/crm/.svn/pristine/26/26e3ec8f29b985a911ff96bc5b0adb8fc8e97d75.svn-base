<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">	
								<label class="col-sm-3 control-label">姓名：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="employeeId" id="employeeId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>	
								<label class="col-sm-3 control-label">员工级别：</label>
								<div class="col-sm-3">
									<input id="employeeLevel" name="employeeLevel" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">时薪：</label>
								<div class="col-sm-3">
									<input id="employeeSalaryHour" name="employeeSalaryHour" class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">投入百分比：</label>
								<div class="col-sm-3">
									<input id="laborRate" name="laborRate" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">投入开始时间：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="laborBeginTime">
							            <input class="form-control" type="text" name="laborBeginTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>	
								<label class="col-sm-3 control-label">投入结束时间：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="laborEndTime">
							            <input class="form-control" type="text" name="laborEndTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">合计天数：</label>
								<div class="col-sm-3">
									<input id="laborTotalDayNum" name="laborTotalDayNum" class="form-control" type="text">
								</div>	
								<label class="col-sm-3 control-label">合计工时数：</label>
								<div class="col-sm-3">
									<input id="laborTotalHourNum" name="laborTotalHourNum" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">人工成本合计：</label>
								<div class="col-sm-3">
									<input id="laborTotalCost" name="laborTotalCost" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">人工的成本总计：</label>
								<div class="col-sm-3">
									<input id="laborGrandTotalCost" name="laborGrandTotalCost" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
									<textarea id="laborRemarks" name="laborRemarks" class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							    </div>
