<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">	
								<label class="col-sm-3 control-label">客户名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="customerId" id="customerId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
								<label class="col-sm-3 control-label">业务名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="businessId" id="businessId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">项目名称：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>	
								<label class="col-sm-3 control-label">项目类别：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentProjectCagegory" id="assignmentProjectCagegory"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">项目经理：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentPm" id="assignmentPm"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							
								<label class="col-sm-3 control-label">申请人：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentPerson" id="assignmentPerson"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">所在部门：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="deptId" id="deptId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>	
								<label class="col-sm-3 control-label">岗位：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="jobId" id="jobId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">任务名称：</label>
								<div class="col-sm-3">
									<input id="assignmentTaskName" name="assignmentTaskName" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">任务完成情况：</label>
								<div class="col-sm-3">
									<input id="assignmentTaskPerformance" name="assignmentTaskPerformance" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">任务委托时间：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="assignmentBeginTime">
							            <input class="form-control" type="text" name="assignmentBeginTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>	
								<label class="col-sm-3 control-label">要求完成时间：</label>
								<div class="col-sm-3">
								    <div class="input-group date" id="assignmentEndTime">
							            <input class="form-control" type="text" name="assignmentEndTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">委托部门：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentDept" id="assignmentDept"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
								<label class="col-sm-3 control-label">委托人：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentPrincipal" id="assignmentPrincipal"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">承接部门：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentRecipientDept" id="assignmentRecipientDept"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								    </div>
								<label class="col-sm-3 control-label">承接人：</label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="assignmentRecipient" id="assignmentRecipient"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-3 control-label">委托任务描述：</label>
								<div class="col-sm-9">
									<textarea id="assignmentTaskDescription" name="assignmentTaskDescription" class="form-control" rows="3"></textarea>
								</div>	
							</div>
							
							<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								    <textarea id="assignmentRemarks" name="assignmentRemarks" class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">
							        <div class="columns pull-right col-md-2">
									<button type="button" class="btn  btn-danger">取消</button>
								    </div>
								    <div class="columns pull-right col-md-2">
									<button type="submit" class="btn btn-primary columns" >提交</button>
									 </div>
							        <!--<button type="submit" class="btn  btn-danger" data-toggle="modal"
									data-target="#myModal">提交</button> --> 
							</div>
