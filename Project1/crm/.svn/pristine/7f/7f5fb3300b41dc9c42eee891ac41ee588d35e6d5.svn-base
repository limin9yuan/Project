<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="Delivery_ID" name="Delivery_ID" th:value="${delivery.Delivery_ID}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">合同编号：</label>
								<div class="col-sm-8">
									<input id="contractId" name="contractId" th:value="${delivery.contractId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">交付人：</label>
								<div class="col-sm-8">
									<input id="deliveryPersonName" name="deliveryPersonName" th:value="${delivery.deliveryPersonName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">交付时间：</label>
								<div class="col-sm-8">
									<input id="deliveryDate" name="deliveryDate" th:value="${delivery.deliveryDate}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">交付内容：</label>
								<div class="col-sm-8">
									<input id="deliveryContent" name="deliveryContent" th:value="${delivery.deliveryContent}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">合同状态：</label>
								<div class="col-sm-8">
									<input id="deliveryStatus" name="deliveryStatus" th:value="${delivery.deliveryStatus}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">附件：</label>
								<div class="col-sm-8">
									<input id="deliveryAttachment" name="deliveryAttachment" th:value="${delivery.deliveryAttachment}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-8">
									<input id="deliveryRmarks" name="deliveryRmarks" th:value="${delivery.deliveryRmarks}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="deliveryOperator" name="deliveryOperator" th:value="${delivery.deliveryOperator}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作时间：</label>
								<div class="col-sm-8">
									<input id="deliveryOperateTime" name="deliveryOperateTime" th:value="${delivery.deliveryOperateTime}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/system/delivery/edit.js">
	</script>
</body>
</html>
