<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<!--
						<input id="Project_ID" name="Project_ID" th:value="${salesProject.Project_ID}"  type="hidden">
						-->
						<input id="salesProject" name="salesProject" th:value="${projectId}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectBusiness}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectName}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectSales}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectManager}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectBeginDate}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectEndDate}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectGategory}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectPhase}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectOldId}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectDescription}" class="form-control" type="text" type="hidden">
						<input id="salesProject" name="salesProject" th:value="${projectRemarks}" class="form-control" type="text" type="hidden">
							<div class="form-group">
								<label class="col-sm-2 control-label">业务名称：<span class="mustin">*</span></label>
								<div class="col-sm-10">
									<select data-placeholder="请选择" name="projectBusiness" id="projectBusiness"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option> 	  
									</select>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-2 control-label">项目名称：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="projectName" name="projectName"  class="form-control" type="text">
								</div>
							    <label class="col-sm-2 control-label">销售负责人：<span class="mustin">*</span></label>
							    <div class="col-sm-4">
								<select data-placeholder="请选择" name="projectSales" id="projectSales"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option> 
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">售前经理：<span class="mustin">*</span></label>
								<div class="col-sm-4">
								<select data-placeholder="请选择" name="projectManager" id="projectManager"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>  
									</select>
								</div>	
								<label class="col-sm-2 control-label">开始时间：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<div class="input-group date" id="projectBeginDate">
							            <input class="form-control" type="text" name="projectBeginDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-2 control-label">结束时间：<span class="mustin">*</span></label>
								<div class="col-sm-4">
							        <div class="input-group date" id="projectEndDate">
							            <input class="form-control" type="text" name="projectEndDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
							    </div>
								<label class="col-sm-2 control-label">项目类型：<span class="mustin">*</span></label>
								<div class="col-sm-4">
								<select data-placeholder="请选择" name="projectGategory" id="projectGategory"
										class="form-control" tabindex="2"
										style="width: 100%">
									<option value="">请选择</option>  	  
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">项目阶段：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="projectPhase" name="projectPhase"  class="form-control" type="text">
								</div>	
								<label class="col-sm-2 control-label">旧项目编号：</label>
								<div class="col-sm-4">
									<input id="projectOldId" name="projectOldId"  class="form-control" type="text">
								</div>	
							</div>
							<div class="form-group">	
								<label class="col-sm-2 control-label">项目描述：</label>
								<div class="col-sm-10">
								    <textarea id="projectDescription" name="projectDescription" class="form-control" rows="3"></textarea>
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-2 control-label">备注：</label>
								<div class="col-sm-10">
								<textarea id="projectRemarks" name="projectRemarks"  class="form-control" rows="5"></textarea>									
								</div>
							</div>
							
							<div class="form-group">	
							   <div class="form-group">	
								   <div class="columns pull-right col-md-2">
										<button type="button" class="btn btn-danger" onclick="closeWin()">取消</button>
								   </div>
								   <div class="columns pull-right col-md-2">
										<button type="submit" class="btn btn-primary columns" >提交</button>
								  </div>
							 </div>
						    </div>	
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/sales/salesProject/edit.js">
	</script>
</body>
</html>
