	<delete id="remove">
		delete from approval_travel where Travel_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from approval_travel where Travel_ID in 
		<foreach item="travelId" collection="array" open="(" separator="," close=")">
