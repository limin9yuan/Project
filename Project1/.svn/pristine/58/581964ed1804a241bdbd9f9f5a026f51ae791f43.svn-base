						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/payment/paid/add.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/paid/paidmentType.js"></script>
