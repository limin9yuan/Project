<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
	<form id="contractForm"
			th:action="@{/contract/contract/export}" method="get">
			<input type="hidden" name="customerId" />
			<input type="hidden" name="businessId" />
			<input type="hidden" name="projectId" />
		</form>
		<div class="col-sm-12">
			<div class="ibox float-e-margins">
				<div class="ibox-content">
					<form class="form-horizontal m-t" id="signupForm">
						<div class="form-group">
							<label class="col-sm-2 control-label">客户名称：</label>
							<div class="col-sm-4">
								<select data-placeholder="全部" name="customerId" id="customerId"
									class="form-control chosen-select" tabindex="1"
									style="width: 100%">
									<option value="">全部</option>
								</select>
							</div>
							<label class="col-sm-2 control-label">业务名称：</label>
							<div class="col-sm-4">
								<select data-placeholder="全部" name="businessId" id="businessId"
									class="form-control chosen-select" tabindex="2"
									style="width: 100%">
									<option value="">全部</option>
								</select>
							</div>
						</div>
						<div class="form-group">
						<label class="col-sm-2 control-label">项目名称：</label>
							<div class="col-sm-4">
								<select data-placeholder="全部" name="projectId" id="projectId"
									class="form-control chosen-select" tabindex="2"
									style="width: 100%">
									<option value="">全部</option>
								</select>
							</div>
						</div>
						<div class="text-right form-group">
							<div class="columns pull-right col-md-2">
								<button type="button" class="btn btn-danger" onclick="reLoad()">查询</button>
								<button type="reset" class="btn btn-primary" onclick="resetSelect()">
									<i class="fa fa-refresh" aria-hidden="true"></i>重置
								</button>
							</div>
						</div>
					</form>
					<div class="fixed-table-toolbar">
						<div class="columns pull-left">
							<!--<button shiro:hasPermission="contract:contract:add" type="button"-->
								<!--class="btn  btn-primary" onclick="add()">-->
								<!--<i class="fa fa-plus" aria-hidden="true"></i>新增-->
							<!--</button>-->
<!-- 							用的是合同起草的导入 -->
							<button shiro:hasPermission="contract:contract:dataImport"
								type="button" class="btn btn-primary" name="excelinsertbtn">
								<i class="fa fa-import" aria-hidden="true"></i>导入
							</button>
							<button class="btn  btn-danger"
								onclick="window.open('/contract/contract/downloadTemplate')"
								type="button">
								<i class="glyphicon glyphicon-save-file "></i> 下载模板
							</button>
							<!-- <a shiro:hasPermission="contract:contract:batchRemove"
								type="button" class="btn  btn-danger" onclick="">
								<i class="fa" aria-hidden="true"></i>列表显示项配置
							</a> -->
						</div>
					</div>
					<table id="exampleTable" data-mobile-responsive="true">
					</table>
				</div>
			</div>
		</div>
	</div>
	<!-- 导入按钮弹出框 -->
	<div name="contractFile_windows" class="modal fade" tabindex="-1"
		data-width="50%">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal"
				aria-hidden="true"></button>
			<h4 class="modal-title">客服记录信息导入</h4>
		</div>
		<div class="modal-body">
			<div class="row">
				<div class="form-group">
					<div class="col-sm-4">
						<div class="file-manager">
							<button type="button" class="layui-btn" id="contractDeliveryImport"
								style="width: 100%;">
								<i class="fa fa-cloud"></i>上传文件
							</button>
							<input id="contractDeliveryFile" name="contractDeliveryFile" type="hidden">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!--shiro控制bootstraptable行内按钮看见性 来自bootdo的创新方案 -->
	<div>
		<script type="text/javascript">
				var s_edit_h = 'hidden';
				var s_remove_h = 'hidden';
				var s_resetPwd_h = 'hidden';
			</script>
	</div>
	<div shiro:hasPermission="contract:contract:editDelivery">
		<script type="text/javascript">
				s_edit_h = '';
			</script>
	</div>
	<!--<div shiro:hasPermission="contract:contract:batchRemove">-->
		<!--<script type="text/javascript">-->
				<!--var s_remove_h = '';-->
			<!--</script>-->
	<!--</div>-->
	<div shiro:hasPermission="contract:contract:resetPwd">
		<script type="text/javascript">
				var s_resetPwd_h = '';
			</script>
	</div>
	</div>
	<div th:include="include :: footer"></div>
	<script type="text/javascript" src="/js/appjs/contract/contract/contractDelivery.js"></script>
</body>
</html>
