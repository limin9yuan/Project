<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<div class="form-group">
<!-- 								<label class="col-sm-2 control-label">合同编号：</label> -->
<!-- 								<div class="col-sm-4"> -->
									<input readonly="readonly" id="contractId" name="contractId" class="form-control" 
										type="hidden">
<!-- 								</div> -->
								<label class="col-sm-2 control-label">收款计划时间：<span class="mustin">*</span></label>
								<div class="col-sm-4">
								<div class="input-group date" id="receivableDate">
										<input class="form-control" type="text" name="receivableDate">
										<span class="input-group-addon"> <span
											class="glyphicon glyphicon-calendar"></span>
										</span>
									</div>
								</div>
								<label class="col-sm-2 control-label">收款计划金额：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivablePrice" name="receivablePrice"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">收款负责人：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivableOwner" name="receivableOwner"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">备注：</label>
								<div class="col-sm-10">
								<textarea id="receivableRemarks" name="receivableRemarks"
										class="form-control" rows="3"></textarea>
								</div>
							</div>
							<!-- <div class="form-group">
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="receivableOperator" name="receivableOperator"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">操作时间：</label>
								<div class="col-sm-8">
									<input id="receivableOperateTime" name="receivableOperateTime"
										class="form-control" type="text">
								</div>
							</div> -->
							<div class="form-group">
								<label class="col-sm-9 control-label"></label>
								<div class="col-sm-3">
									<button type="submit" class="btn btn-primary">提交</button>
									<button type="button" class="btn btn-primary" onclick="closeWin()">取消</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/contract/contract/addReceivable.js"></script>
</body>
</html>
