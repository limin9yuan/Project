<!DOCTYPE html>
<html xmlns:th="http://www.w3.org/1999/xhtml">
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="timesheetId" name="timesheetId" th:value="${timesheet.timesheetId}"  type="hidden">

                            <!--<input id="projectName" name="projectName" type="hidden"value="${project.projectId}">-->
							<!--<input id="timesheetProjectCagegory" name="timesheetProjectCagegory" type="hidden" value="${project.projectGategory}">-->
							<!--<input id="timesheetPm" name="timesheetPm" type="hidden"value="${project.projectOwner}">-->

							<div class="form-group">
								<label class="col-sm-3 control-label">委托任务id：</label>
								<div class="col-sm-8">
									<input id="timesheetAssignmentId" name="timesheetAssignmentId" th:value="${timesheet.timesheetAssignmentId}" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">工作内容：</label>
								<div class="col-sm-8">
									<input id="timesheetContent" name="timesheetContent" th:value="${timesheet.timesheetContent}" class="form-control" type="text">
								</div>
							</div>

							<div class="form-group">
								<label class="col-sm-3 control-label">正常工时：</label>
								<div class="col-sm-8">
									<input id="timesheetNormal" name="timesheetNormal" th:value="${timesheet.timesheetNormal}" class="form-control" type="text">
								</div>
							</div>


							<div class="form-group">
								<label class="col-sm-3 control-label">加班工时：</label>
								<div class="col-sm-8">
									<input id="timesheetOvertime" name="timesheetOvertime" th:value="${timesheet.timesheetOvertime}" class="form-control" type="text">
								</div>
							</div>

							<div class="form-group">
								<label class="col-sm-3 control-label">出现问题：</label>
								<div class="col-sm-8">
									<input id="timesheetProblem" name="timesheetProblem" th:value="${timesheet.timesheetProblem}" class="form-control" type="text">
								</div>
							</div>

                            是否计划
							<input type="checkbox"   name="timesheetPlanned"  value="1" >是
							 <input type="checkbox"     name="timesheetPlanned" value="0">否

							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/timesheet/timesheet/edit.js">
	</script>
</body>
</html>
