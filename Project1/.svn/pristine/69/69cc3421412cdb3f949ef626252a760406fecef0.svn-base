	<delete id="remove">
		delete from contract where Contract_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from contract where Contract_ID in 
		<foreach item="contractId" collection="array" open="(" separator="," close=")">
			#{contractId}
		</foreach>
	</delete>
	<!-- 合同名称 -->
