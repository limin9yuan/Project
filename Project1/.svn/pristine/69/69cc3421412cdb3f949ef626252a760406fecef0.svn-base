		  		  <if test="contractDraftPerson != null and contractDraftPerson != ''"> and Contract_Draft_Person = #{contractDraftPerson} </if>
		  		  <if test="contractSales != null and contractSales != ''"> and Contract_Sales = #{contractSales} </if>
