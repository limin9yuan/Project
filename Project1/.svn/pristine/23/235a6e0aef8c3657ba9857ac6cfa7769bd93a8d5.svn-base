												<div class="col-sm-4">
													<input id="expensesTravelCreateTime"
														name="expensesTravelCreateTime" class="form-control"
														type="text">
												</div>
											</div>
										</div>


										<!-- 开票信息 -->


										<div class="tab-pane fade" id="Rechnungs">
											<div class="form-group">
												<label class="col-sm-2 control-label">开票名称：</label>
												<div class="col-sm-4">
													<input id="customerInvoiceName" name="customerInvoiceName"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">纳税人税号：</label>
												<div class="col-sm-4">
													<input id="customerTaxNumber" name="customerTaxNumber"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">开户行：</label>
												<div class="col-sm-4">
													<input id="customerBank" name="customerBank"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">账号：</label>
												<div class="col-sm-4">
													<input id="customerAccountNum" name="customerAccountNum"
														class="form-control" type="text">
												</div>
											</div>


										</div>





										<!-- 联系方式 -->


										<div class="tab-pane fade" id="linkInfo">
											<div class="form-group">
												<label class="col-sm-2 control-label">联系情况：</label>
												<div class="col-sm-4">
													<select data-placeholder="请选择" name="customerContactSta"
														id="customerContactSta" class="form-control chosen-select"
														tabindex="2" style="width: 100%">
														<option value="">请选择</option>
													</select>
												</div>


												<label class="col-sm-2 control-label">企业办公地址：</label>
												<div class="col-sm-4">
													<input id="customerAddress" name="customerAddress"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">电话号码：</label>
												<div class="col-sm-4">
													<input id="customerPhoneNum" name="customerPhoneNum"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">传真：</label>
												<div class="col-sm-4">
													<input id="customerFax" name="customerFax"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">邮编：</label>
												<div class="col-sm-4">
													<input id="customerPostcode" name="customerPostcode"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">官网地址：</label>
												<div class="col-sm-4">
													<input id="customerUrl" name="customerUrl"
														class="form-control" type="text">
												</div>
											</div>


											<div class="form-group">
												<label class="col-sm-2 control-label">企业邮箱：</label>
												<div class="col-sm-4">
													<input id="customerMailbox" name="customerMailbox"
														class="form-control" type="text">
												</div>



												<label class="col-sm-2 control-label">企业负责人：</label>
												<div class="col-sm-4">
													<input id="customerLeader" name="customerLeader"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">职务：</label>
												<div class="col-sm-4">
													<input id="customerJobTitle" name="customerJobTitle"
														class="form-control" type="text">
												</div>
											</div>
										</div>

										<!-- 热点情况 -->


										<div class="tab-pane fade" id="cost">

											<div class="form-group">
												<label class="col-sm-2 control-label">出差任务：</label>
												<div class="col-sm-4">
													<input id="expensesTravelTask" name="expensesTravelTask"
														class="form-control" type="text">
												</div>
												<label class="col-sm-2 control-label">出差补助：</label>
												<div class="col-sm-4">
													<input id="expensesTravelAllowance"
														name="expensesTravelAllowance" class="form-control"
														type="text">
												</div>
											</div>


											<div class="form-group">
												<label class="col-sm-2 control-label">其他费用类型：</label>
												<div class="col-sm-4">
													<input id="expensesTravelType" name="expensesTravelType"
														class="form-control" type="text">
												</div>
												<label class="col-sm-2 control-label">其他费用金额：</label>
												<div class="col-sm-4">
													<input id="expensesTravelPrice" name="expensesTravelPrice"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">预借金额：</label>
												<div class="col-sm-4">
													<input id="expensesTravelPayPrice"
														name="expensesTravelPayPrice" class="form-control"
														type="text">
												</div>
												<label class="col-sm-2 control-label">结算方式：</label>
												<div class="col-sm-4">
													<select data-placeholder="请选择" name="expensesTravelPayMode"
														id="expensesTravelPayMode"
														class="form-control chosen-select" tabindex="2"
														style="width: 100%">
														<option value="">请选择</option>
													</select>
												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-2 control-label">收款人银行：</label>
												<div class="col-sm-4">
													<input id="expensesTravelBank" name="expensesTravelBank"
														class="form-control" type="text">
												</div>
												<label class="col-sm-2 control-label">卡号：</label>
												<div class="col-sm-4">
													<input id="expensesTravelCardNum"
														name="expensesTravelCardNum" class="form-control"
														type="text">
												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-2 control-label">备注：</label>
												<div class="col-sm-4">
													<input id="expensesTravelRemarks"
														name="expensesTravelRemarks" class="form-control"
														type="text">
												</div>
												<label class="col-sm-2 control-label">审批状态：</label>
												<div class="col-sm-4">
													<input id="expensesTravelStatus"
														name="expensesTravelStatus" class="form-control"
														type="text">
												</div>
											</div>

										</div>
										<!-- 生产信息-->


										<div class="tab-pane fade" id="Information">
											<div class="form-group">
												<label class="col-sm-2 control-label">收费收缴率：</label>
												<div class="col-sm-4">
													<input id="customerPaymentRate" name="customerPaymentRate"
														class="form-control" type="text">
												</div>



												<label class="col-sm-2 control-label">企业占全市热化率：</label>
												<div class="col-sm-4">
													<input id="customerHeatingShare"
														name="customerHeatingShare" class="form-control"
														type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">投诉率：</label>
												<div class="col-sm-4">
													<input id="customerComplaintRate"
														name="customerComplaintRate" class="form-control"
														type="text">
												</div>



												<label class="col-sm-2 control-label">供热面积：</label>
												<div class="col-sm-4">
													<input id="customerHeatingArea" name="customerHeatingArea"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">热力站数量：</label>
												<div class="col-sm-4">
													<input id="customerHeatingSourceNumber"
														name="customerHeatingSourceNumber" class="form-control"
														type="text">
												</div>


												<label class="col-sm-2 control-label">蒸汽面积：</label>
												<div class="col-sm-4">
													<input id="customerSteamArea" name="customerSteamArea"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">热水面积：</label>
												<div class="col-sm-4">
													<input id="customerHotWaterArea"
														name="customerHotWaterArea" class="form-control"
														type="text">
												</div>


												<label class="col-sm-2 control-label">自有热源：</label>
												<div class="col-sm-4">
													<input id="customerOwnHeatingSource"
														name="customerOwnHeatingSource" class="form-control"
														type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">外购热源：</label>
												<div class="col-sm-4">
													<input id="customerOutHeatingSource"
														name="customerOutHeatingSource" class="form-control"
														type="text">
												</div>
											</div>
										</div>


										<!-- 能耗 -->


										<div class="tab-pane fade" id="Energieve">

											<div class="form-group">
												<label class="col-sm-2 control-label">热耗：</label>
												<div class="col-sm-4">
													<input id="customerHeatLoss" name="customerHeatLoss"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">水耗：</label>
												<div class="col-sm-4">
													<input id="customerWaterLoss" name="customerWaterLoss"
														class="form-control" type="text">
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">电耗：</label>
												<div class="col-sm-4">
													<input id="customerElectrickLoss"
														name="customerElectrickLoss" class="form-control"
														type="text">
												</div>
											</div>

										</div>
										<!-- 未来规划-->


										<div class="tab-pane fade" id="Zukunft">

											<div class="form-group">
												<label class="col-sm-3 control-label">未来一年新增规划：</label>
												<div class="col-sm-8">
													<textarea id="customerPlanOneYear"
														name="customerPlanOneYear" class="form-control" rows="3"></textarea>

												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-3 control-label">未来两年新增规划：</label>
												<div class="col-sm-8">
													<textarea id="customerPlanTowYear"
														name="customerPlanTowYear" class="form-control" rows="3"></textarea>

												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-3 control-label">未来三年新增规划：</label>
												<div class="col-sm-8">
													<textarea id="customerPlanThreeYear"
														name="customerPlanThreeYear" class="form-control" rows="3"></textarea>

												</div>
											</div>

										</div>
									<!-- 竞争对手 -->


										<div class="tab-pane fade" id="Gegner">
											<div class="wrapper wrapper-content ">
												<div class="col-sm-12">
													<div class="ibox">
														<div class="ibox-body">
															<div class="fixed-table-toolbar">
																<div class="columns pull-left">
																	<button shiro:hasPermission="sales:competitor:add"
																		type="button" class="btn  btn-primary" onclick="add()">
																		<i class="fa fa-plus" aria-hidden="true"></i>添加
																	</button>
																	<button
																		shiro:hasPermission="sales:competitor:batchRemove"
																		type="button" class="btn  btn-danger"
																		onclick="batchRemove()">
																		<i class="fa fa-trash" aria-hidden="true"></i>删除
																	</button>
																</div>

															</div>
															<table id="exampleTable" data-mobile-responsive="true">
															</table>
														</div>
													</div>
												</div>
											</div>
											<!--shiro控制bootstraptable行内按钮看见性 来自bootdo的创新方案 -->
											<div>
												<script type="text/javascript">
													var s_edit_h = 'hidden';
													var s_remove_h = 'hidden';
													var s_resetPwd_h = 'hidden';
												</script>
											</div>
											<div shiro:hasPermission="sales:competitor:edit">
												<script type="text/javascript">
													s_edit_h = '';
												</script>
											</div>
											<div shiro:hasPermission="sales:competitor:batchRemove">
												<script type="text/javascript">
													var s_remove_h = '';
												</script>
											</div>
											<div shiro:hasPermission="sales:competitor:resetPwd">
												<script type="text/javascript">
													var s_resetPwd_h = '';
												</script>
											</div>
										</div>
									<!-- 自定义字段 -->


										<div class="tab-pane fade" id="Custom">

											<div class="form-group">
												<label class="col-sm-2 control-label">字段信息：</label>
												<div class="col-sm-4">
													<input id="opponentProjectType" name="opponentProjectType"
														class="form-control" type="text">
												</div>


												<label class="col-sm-2 control-label">所属分类：</label>
												<div class="col-sm-4">
													<select data-placeholder="请选择" name="       " id="       "
														class="form-control chosen-select" tabindex="2"
														style="width: 100%">
														<option value="">请选择</option>
													</select>
												</div>
											</div>

											<div class="form-group">
												<label class="col-sm-2 control-label">产品分类：</label>
												<div class="col-sm-4">
													<select data-placeholder="请选择" name="       " id="       "
														class="form-control chosen-select" tabindex="2"
														style="width: 100%">
														<option value="">请选择</option>
													</select>
												</div>

												<label class="col-sm-2 control-label">产品名称：</label>
												<div class="col-sm-4">
													<input id="opponentProductType" name="opponentProductType"
														class="form-control" type="text">
												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-2 control-label">价格：</label>
												<div class="col-sm-4">
													<input id="opponentProductPrice"
														name="opponentProductPrice" class="form-control"
														type="text">
												</div>
											</div>
											<div class="form-group">
												<label class="col-sm-2 control-label">备注：</label>
												<div class="col-sm-8">
													<textarea id="customerRemarks" name="customerRemarks"
														class="form-control" rows="3"></textarea>

												</div>
											</div>





										</div>
									</div>
							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript"
		src="/js/appjs/sales/companyCustomer/add.js"></script>
	<script type="text/javascript"
											src="/js/appjs/sales/companyCustomer/competitor.js"></script>
	</script>
</body>
</html>
