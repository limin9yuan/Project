									<input id="bugRecorder" name="bugRecorder" th:value="${bugCategory.bugRecorder}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">修改时间：</label>
								<div class="col-sm-8">
									<input id="bugRecordTime" name="bugRecordTime" th:value="${bugCategory.bugRecordTime}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">创建人：</label>
								<div class="col-sm-8">
									<input id="bugCreator" name="bugCreator" th:value="${bugCategory.bugCreator}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">创建时间：</label>
