<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">

	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">

							<input id="TimeSheet_Project_Cagegory" type="hidden" name="TimeSheet_Project_Cagegory">
							<input id="customerId" type="hidden" name="customerId">
							<input id="assignmentPm" type="hidden" name="assignmentPm">
							<input id="timesheetPm" type="hidden" name="timesheetPm">
							<input id="projectOwner" type="hidden" name="projectOwner">
							<input id="assignmentProjectCagegory" type="hidden" name="assignmentProjectCagegory">
							<input id="timesheetProjectCagegory" type="hidden" name="timesheetProjectCagegory">
							<div class="form-group">
								<label class="col-sm-3 control-label">项目名称：<span class="mustin">*</span></label>
								<div class="col-sm-9">
									<select data-placeholder="请选择" name="projectId"
											id="projectId" class="form-control chosen-select"
											tabindex="2" style="width: 100%">
									</select>
								</div>
							</div>






							<div class="form-group">
								<label class="col-sm-3 control-label">项目描述：<span class="mustin">*</span></label>
								<div class="col-sm-8">
									<input id="projectDescription" name="projectDescription" class="form-control"  readonly>
								</div>
							</div>









							<div class="form-group">
								<label class="col-sm-3 control-label">项目经理：<span class="mustin">*</span></label>
								<div class="col-sm-8">
									<input id="employeeName" name="employeeName" class="form-control"  readonly>
								</div>
							</div>

							<div class="form-group">
								<label class="col-sm-3 control-label">项目名称：<span class="mustin">*</span></label>
								<div class="col-sm-8">
									<input id="projectName" name="projectName" class="form-control"  readonly>
								</div>
							</div>






									<div class="form-group">
										<label class="col-sm-3 control-label">任务委托时间：<span class="mustin">*</span></label>
										<div class="col-sm-3">
											<div class="input-group date" id="assignmentBeginTime">
												<input class="form-control" type="text" name="assignmentBeginTime">
												<span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
											</div>
										</div>
										<label class="col-sm-3 control-label">要求完成时间：<span class="mustin">*</span></label>
										<div class="col-sm-3">
											<div class="input-group date" id="assignmentEndTime">
												<input class="form-control" type="text" name="assignmentEndTime">
												<span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-3 control-label">委托部门：<span class="mustin">*</span></label>
										<div class="col-sm-3">
											<select data-placeholder="请选择" name="assignmentDept" id="assignmentDept"
													class="form-control chosen-select" tabindex="2"
													style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-3 control-label">委托人：<span class="mustin">*</span></label>
										<div class="col-sm-3">
											<select data-placeholder="请选择" name="assignmentPrincipal" id="assignmentPrincipal"
													class="form-control chosen-select" tabindex="2"
													style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-3 control-label">委托任务描述：<span class="mustin">*</span></label>
										<div class="col-sm-9">
											<textarea id="assignmentTaskDescription" name="assignmentTaskDescription" class="form-control" rows="3"></textarea>
										</div>
									</div>



									<div class="form-group">
										<label class="col-sm-3 control-label">工作内容：<span class="mustin">*</span></label>
										<div class="col-sm-9">
											<textarea id="timesheetContent" name="timesheetContent" class="form-control" rows="3"></textarea>
										</div>
									</div>


							<div class="form-group">
								<label class="col-sm-3 control-label">正常工时：</label>
								<div class="col-sm-8">
									<input id="timesheetNormal" name="timesheetNormal" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">加班工时：</label>
								<div class="col-sm-8">
									<input id="timesheetOvertime" name="timesheetOvertime" class="form-control" type="text">
								</div>
							</div>




									<div class="form-group">
										<label class="col-sm-3 control-label">是否计划：</label>
										<div class="col-sm-8">
											<input type="radio"   name="timesheetPlanned"  value="1" >是
											<input type="radio"     name="timesheetPlanned" value="0">否
										</div>
									</div>




							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>

						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/timesheet/timesheet/add.js">
	</script>
</body>
</html>
