<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="Purchase_ID" name="Purchase_ID" th:value="${purchase.Purchase_ID}"  type="hidden">
<div class="form-group">	
								<label class="col-sm-3 control-label">项目预算编号：</label>
								<div class="col-sm-8">
									<input id="budgetId" name="budgetId" th:value="${purchase.budgetId}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">	
								<label class="col-sm-3 control-label">采购类型：</label>
								<div class="col-sm-8">
									<input id="purchaseType" name="purchaseType" th:value="${purchase.purchaseType}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">采购金额预估：</label>
								<div class="col-sm-8">
									<input id="purchasePrice" name="purchasePrice" th:value="${purchase.purchasePrice}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">预估说明：</label>
								<div class="col-sm-8">
									<input id="purchaseDescription" name="purchaseDescription" th:value="${purchase.purchaseDescription}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户承担：</label>
								<div class="col-sm-8">
									<input id="purchaseCustomerRate" name="purchaseCustomerRate" th:value="${purchase.purchaseCustomerRate}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">项目组承担：</label>
								<div class="col-sm-8">
									<input id="purchaseProjectRate" name="purchaseProjectRate" th:value="${purchase.purchaseProjectRate}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-8">
									<input id="purchaseRemarks" name="purchaseRemarks" th:value="${purchase.purchaseRemarks}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="purchaseOperator" name="purchaseOperator" th:value="${purchase.purchaseOperator}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">操作时间：</label>
								<div class="col-sm-8">
									<input id="purchaseOperateTime" name="purchaseOperateTime" th:value="${purchase.purchaseOperateTime}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/budget/purchase/edit.js">
	</script>
</body>
</html>
