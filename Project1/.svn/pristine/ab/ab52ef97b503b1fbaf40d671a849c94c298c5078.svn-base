<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="holidayId" name="holidayId" th:value="${holiday.holidayId}"  type="hidden">
							<div class="form-group">
								<label class="col-sm-3 control-label">假日名称：</label>
								<div class="col-sm-8">
									<input id="holidayName" name="holidayName" th:value="${holiday.holidayName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-3 control-label">起始日：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="startDay">
										<input th:value="${holiday.startDay}" class="form-control" type="text"
					                        name="startDay" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">结束日：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="endDay">
										<input th:value="${holiday.endDay}" class="form-control" type="text"
					                        name="endDay" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">上午上班时间：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="startWorkAM">
										<input th:value="${holiday.startWorkAM}" class="form-control" type="text"
					                        name="startWorkAM" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">上午下班时间：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="endWorkAM">
										<input th:value="${holiday.endWorkAM}" class="form-control" type="text"
					                        name="endWorkAM" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">下午上班时间：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="startWorkPM">
										<input th:value="${holiday.startWorkPM}" class="form-control" type="text"
					                        name="startWorkPM" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">下午下班时间：</label>
								<div class="col-sm-8">
									<div class="col-sm-1 input-group date" id="endWorkPM">
										<input th:value="${holiday.endWorkPM}" class="form-control" type="text"
					                        name="endWorkPM" style="width:100px"> <span
					                        class="input-group-addon"> <span
					                        class="glyphicon glyphicon-calendar"></span>
					                    </span>
					                    </input>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/workDay/edit.js">
	</script>
</body>
</html>
