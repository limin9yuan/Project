<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						    <div class="form-group">
								<label class="col-sm-2 control-label">收款计划ID：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="labelId" name="labelId" type="hidden">
									<div style="float: left;" id="labelName"></div>
									<input id=receivableId name="receivableId" class="form-control" type="text"
										style="cursor: pointer;" onclick="addReceivable()"
										readonly="readonly" placeholder="">
								</div>
								<label class="col-sm-2 control-label">合同编号：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="contractId" name="contractId"
										class="form-control" type="text" readonly>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">业务编号：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="businessId" name="businessId"
										class="form-control" type="text" readonly>
								</div>
								<label class="col-sm-2 control-label">应收款金额：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivablePrice" name="receivablePrice"
										class="form-control" type="text" readonly>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">收款计划时间：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivableDate" name="receivableDate"
										class="form-control" type="text" disabled>
								</div>
								<label class="col-sm-2 control-label">收款延迟时间：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receiptDelayTime" name="receiptDelayTime"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">回款率%：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="reimbursementRate" name="reimbursementRate"
										class="form-control" type="text" readonly>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">收款时间：：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<div class="input-group date" id="receivedTime">
										<input class="form-control" type="text" name="receivedTime">
										<span class="input-group-addon"> <span
											class="glyphicon glyphicon-calendar"></span>
										</span>
									</div>
								</div>
								<label class="col-sm-2 control-label">收款金额：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivedPrice" name="receivedPrice"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">收款账户：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivedCardNumber" name="receivedCardNumber"
										class="form-control" type="text"> <!-- onblur="CheckBankNo($('#receivedCardNumber'));" -->
								   <span id="banknoInfo" style="color:#dd0000"></span>
								</div>
								<label class="col-sm-2 control-label">款项类型：<span class="mustin">*</span></label>
								<div class="col-sm-4">
									<input id="receivedType" name="receivedType"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">	
								<label class="col-sm-2 control-label">合同状态：<span class="mustin">*</span></label>
								<div class="col-sm-9">
									<label class="radio-inline"> 
									    <input type="radio" name="receivedContractStatus" value="0" /> 未开始
									</label> 
									<label class="radio-inline"> 
									    <input type="radio" name="receivedContractStatus" value="1" /> 交付中
									</label>
									<label class="radio-inline"> 
									    <input type="radio" name="receivedContractStatus" value="2" /> 已完成
									</label>
								</div>
						    </div>
						    <div class="form-group">
								<label class="col-sm-2 control-label">备注：</label>
								<div class="col-sm-10">
									<textarea id="receivedRemarks" name="receivedRemarks"
										class="form-control" rows="3"></textarea>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">主送人：</label>
								<div class="col-sm-8">
									<input id="mainPersonId" name="mainPersonId" type="hidden">
									<input id="isMainPerson" name="isMainPerson" type="hidden">
									<div style="float: left;" id="sendPerson"></div>
									<div style="float: left;">
										<button
											type="button" class="btn btn-danger btn-xs"
											id="sendPersonBtn" var="sendPersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addSendPerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
						</div>
						<div class="form-group">
							<label class="col-sm-2 control-label">抄送人：</label>
							<div class="col-sm-8">
								<input id="copyPersonId" name="copyPersonId" type="hidden">
								<input id="isCopyPerson" name="isCopyPerson" type="hidden">
								<div style="float: left;" id="receivePerson"></div>
								<div style="float: left;">
									<button type="button" class="btn btn-danger btn-xs"
										id="receivePersonBtn" var="receivePersonBtn"
										style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
										onclick="addRecivePerson()">
										<span class="glyphicon glyphicon-plus"></span>
									</button>
								</div>
							</div>
						</div>
						 <div class="form-group">
								 <div class="columns pull-right col-md-2">
										<button type="button" class="btn btn-danger" onclick="closeWin()">取消</button>
								   </div>
								   <div class="columns pull-right col-md-2">
										<button type="submit" class="btn btn-primary columns" >提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/payment/received/add.js"></script>
	<script type="text/javascript" src="/js/common.js"></script>
	<script type="text/javascript" src="/js/appjs/payment/received/receivedType.js"></script>
	<script type="text/javascript" src="/js/appjs/common/mainCopyPerson/mainPersonAndCopyPerson.js"></script>
</body>
</html>
