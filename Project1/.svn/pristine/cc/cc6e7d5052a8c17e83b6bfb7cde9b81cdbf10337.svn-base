<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<input id="purchaseIds" name="purchaseIds" class="form-control" type="hidden" value="-1">

							<div class="form-group">
								<label class="col-sm-3 control-label">项目名称：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								   <select data-placeholder="请选择" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									</select>
								</div>
								<label class="col-sm-3 control-label">申购部门：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								   <select data-placeholder="请选择" name="purchaseDept" id="purchaseDept"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">申购人：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="purchasePerson" id="purchasePerson"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
									</select>
								</div>
								<label class="col-sm-3 control-label">收货人：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseConsignee" name="purchaseConsignee" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">申购时间：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<div class="input-group date" id="purchaseDate">
							            <input class="form-control" type="text" name="purchaseDate">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
								<label class="col-sm-3 control-label">要求交货时间：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<div class="input-group date" id="purchaseDeliveryTime">
							            <input class="form-control" type="text" name="purchaseDeliveryTime">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">交货地点：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseDeliveryPlace" name="purchaseDeliveryPlace" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">联系电话：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchasePhoneNumber" name="purchasePhoneNumber" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">物品名称：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseName" name="purchaseName" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">规格/配置：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseConfig" name="purchaseConfig" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">品牌：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseBrand" name="purchaseBrand" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">型号：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseMode" name="purchaseMode" class="form-control" type="text">
								</div>
							</div>

							<div class="form-group">
								<label class="col-sm-3 control-label">单位：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseUnit" name="purchaseUnit" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">数量：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseNumber" name="purchaseNumber" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">预算单价：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseUnitPrice" name="purchaseUnitPrice" class="form-control" type="text">
								</div>
								<label class="col-sm-3 control-label">总价：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="purchaseTotalPrice" name="purchaseTotalPrice" class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								    <textarea id="purchaseRemarks" name="purchaseRemarks" class="form-control" rows="5"></textarea>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">主送人：</label>
								<div class="col-sm-8">
									<input id="mainPersonId" name="mainPersonId" type="hidden">
									<input id="isMainPerson" name="isMainPerson" type="hidden">
									<div style="float: left;" id="sendPerson"></div>
									<div style="float: left;">
										<button
											type="button" class="btn btn-danger btn-xs"
											id="sendPersonBtn" var="sendPersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addSendPerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">抄送人：</label>
								<div class="col-sm-8">
									<input id="copyPersonId" name="copyPersonId" type="hidden">
									<input id="isCopyPerson" name="isCopyPerson" type="hidden">
									<div style="float: left;" id="receivePerson"></div>
									<div style="float: left;">
										<button type="button" class="btn btn-danger btn-xs"
											id="receivePersonBtn" var="receivePersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addRecivePerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							 <div class="form-group">
								 <div class="columns pull-right col-md-2">
									 <button type="submit" class="btn btn-primary columns" >提交</button>

										<button type="button" class="btn btn-danger" onclick="closeWin()">取消</button>

								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/common.js"></script>
	<script type="text/javascript" src="/js/appjs/approval/purchase/add.js"></script>
	<script type="text/javascript" src="/js/appjs/common/mainCopyPerson/mainPersonAndCopyPerson.js"></script>
</body>
</html>
