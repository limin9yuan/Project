							<table  width="708"  id="tabProduct">

								<tr>
									<td width="20" style="text-align:center;"  align="center"   bgcolor="#EFEFEF" Name="Num"><input type="checkbox" name="checkbox" value="checkbox" /></td>
									<td width="60" style="text-align:center;"  bgcolor="#EFEFEF" Name="Num"  style=" text-align:center">序号</td>

									<td width="103" bgcolor="#EFEFEF" style="text-align:center;" Name="purchaseName" EditType="TextBox">物品名称</td>
									<td width="103" bgcolor="#EFEFEF" style="text-align:center;"  Name="purchaseConfig" EditType="TextBox">规格/配置</td>
