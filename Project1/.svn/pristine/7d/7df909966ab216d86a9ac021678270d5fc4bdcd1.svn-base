<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<!--  <input id="Project_ID" name="Project_ID" th:value="${project.Project_ID}"  type="hidden">
						-->
						<input id="projectId" name="projectId" th:value="${projectId}" class="form-control" type="text" type="hidden">
							<div class="form-group">
								<label class="col-sm-3 control-label">项目名称：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="projectName" name="projectName" class="form-control" type="text" disabled>
								</div>
								<label class="col-sm-3 control-label">客户名称：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="customerId" id="customerId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目经理：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectOwner" id="projectOwner"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
								<label class="col-sm-3 control-label">研发经理：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectManager" id="projectManager"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目类型：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectGategory" id="projectGategory"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
								<label class="col-sm-3 control-label">销售负责人：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="projectSales" id="projectSales"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">是否签订合同：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="deliveryStatus" id="deliveryStatus"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">合同及中心占比：</label>
								<div class="col-sm-9">
									<input id="ContractCenterratio" name="ContractCenterratio"
									 class="form-control" type="text" readonly="readonly">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目开始时间：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<div class="input-group date" id="projectBeginDate">
							            <input class="form-control" type="text" name="projectBeginDate" readonly="readonly">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
								</div>
								<label class="col-sm-3 control-label">项目结束时间：<span class="mustin">*</span></label>
								<div class="col-sm-3">
							        <div class="input-group date" id="projectEndDate">
							            <input class="form-control" type="text" name="projectEndDate" readonly="readonly">
							            <span class="input-group-addon">
							               <span class="glyphicon glyphicon-calendar"></span>
							            </span>
							        </div>
							    </div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目研发开始时间：</label>
								<div class="col-sm-3">
									<div class="input-group date" id="projectDevelopmentBeginDate">
										<input class="form-control" type="text" readonly="readonly"
											name="projectDevelopmentBeginDate"> <span
											class="input-group-addon"> <span
											class="glyphicon glyphicon-calendar"></span>
										</span>
									</div>
								</div>
								<label class="col-sm-3 control-label">项目研发结束时间：</label>
								<div class="col-sm-3">
									<div class="input-group date" id="projectDevelopmentEndDate">
										<input class="form-control" type="text" readonly="readonly"
											name="projectDevelopmentEndDate"> <span
											class="input-group-addon"> <span
											class="glyphicon glyphicon-calendar"></span>
										</span>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目阶段：<span class="mustin">*</span></label>
								<div class="col-sm-3">
									<input id="projectPhase" name="projectPhase" class="form-control" type="text" readonly="readonly">
								</div>
								<label class="col-sm-3 control-label">流程种类：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="fllowType" id="fllowType"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">旧项目编号：</label>
								<div class="col-sm-3">
									<input id="projectOldId" name="projectOldId" class="form-control" type="text" readonly="readonly">
								</div>
								<label class="col-sm-3 control-label">是否分包：<span class="mustin">*</span></label>
								<div class="col-sm-3">
								    <select data-placeholder="请选择" name="ifOutSource" id="ifOutSource"
									 class="form-control chosen-select" tabindex="2"
										style="width: 100%" disabled>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">项目描述：</label>
								<div class="col-sm-9">
								    <textarea id="projectDescription" name="projectDescription"
								     class="form-control" rows="3" readonly="readonly"></textarea>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-9">
								<textarea id="projectRemarks" name="projectRemarks"
								 class="form-control" rows="5" readonly="readonly"></textarea>
								</div>
							</div>
							<div class="form-group">
							<label class="col-sm-3 control-label">部门名称：</label>
								<div class="col-sm-3">
									<input id="deptId" name="deptId" class="hidden"> <input
										id="deptName" name="deptName" class="form-control" type="text"
										style="cursor: pointer;" onclick="openDept('deptId')"
										readonly="readonly" placeholder="部门名称">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">主送人：</label>
								<div class="col-sm-8">
									<input id="mainPersonId" name="mainPersonId" type="hidden">
									<input id="isMainPerson" name="isMainPerson" type="hidden">
									<div style="float: left;" id="sendPerson"></div>
									<div style="float: left;">
										<button
											type="button" class="btn btn-danger btn-xs"
											id="sendPersonBtn" var="sendPersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addSendPerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">抄送人：</label>
								<div class="col-sm-8">
									<input id="copyPersonId" name="copyPersonId" type="hidden">
									<input id="isCopyPerson" name="isCopyPerson" type="hidden">
									<div style="float: left;" id="receivePerson"></div>
									<div style="float: left;">
										<button type="button" class="btn btn-danger btn-xs"
											id="receivePersonBtn" var="receivePersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addRecivePerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/project/project/edit.js">
	</script>
</body>
</html>
