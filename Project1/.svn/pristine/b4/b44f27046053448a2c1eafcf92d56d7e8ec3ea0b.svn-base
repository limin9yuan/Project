<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="payoutId" name="payoutId" th:value="${payout.payoutId}"  type="hidden">

							<div class="form-group">
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>企业客户编号：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="customerId" id="customerId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>业务编号：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="businessId" id="businessId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
							</div>

														<div class="form-group">
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>项目编号：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="projectId" id="projectId"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>申请人姓名：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="payoutPerson" id="payoutPerson"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>请款用途：</label>
								<div class="col-sm-4">
									<input id="payoutUseage" name="payoutUseage" class="form-control" type="text">
								</div>
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>请款金额：</label>
								<div class="col-sm-4">
									<input id="payoutPrice" name="payoutPrice" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>结算方式：</label>
								<div class="col-sm-4">
									<select data-placeholder="请选择" name="payoutMeans" id="payoutMeans"
										class="form-control chosen-select" tabindex="2"
										style="width: 100%">
										<option value="">请选择</option>
									</select>
								</div>
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>收款人银行：</label>
								<div class="col-sm-4">
									<input id="payoutBeneficiaryBank" name="payoutBeneficiaryBank" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>银行卡号：</label>
								<div class="col-sm-4">
									<input id="payoutCardNumber" name="payoutCardNumber" class="form-control" type="text">
								</div>
								<label class="col-sm-2 control-label"><span
											class="mustin">*&nbsp;</span>出差编号：</label>
								<div class="col-sm-4">
									<input data-placeholder="请选择" name="payoutRelatedContractId"
										id="payoutRelatedContractId"
										class="form-control chosen-select" readonly="readonly" onclick="relateTravel()"
										style="width: 100%">
									</input>
								</div>
							</div>
							<!-- <div class="form-group">
								<label class="col-sm-2 control-label"><span
									class="mustin">*&nbsp;</span>审批状态：</label>
								<div class="col-sm-4">
									<label class="radio-inline"> <input type="radio"
										name="payoutApprovalStatus" value="1" /> 已审批
									</label> <label class="radio-inline"> <input type="radio"
										name="payoutApprovalStatus" value="0" checked="checked" /> 未审批
									</label>
								</div>
							</div> -->
							<div class="form-group">
								<label class="col-sm-2 control-label">备注：</label>
								<div class="col-sm-10">
									<textarea id="payoutRemarks" name="payoutRemarks" class="form-control" rows="3"></textarea>
								</div>
							</div>
				<!-- 								<div class="form-group">
								<label class="col-sm-3 control-label">实际请款结果：</label>
								<div class="col-sm-8">
									<input id="payoutResult" name="payoutResult" class="form-control" type="text">
								</div>
							</div>

														<div class="form-group">
								<label class="col-sm-3 control-label">审批状态：</label>
								<div class="col-sm-8">
									<input id="payoutApprovalStatus" name="payoutApprovalStatus" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-3 control-label">操作人：</label>
								<div class="col-sm-8">
									<input id="payoutOperator" name="payoutOperator" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-3 control-label">修改时间：</label>
								<div class="col-sm-8">
									<input id="payoutOperateTime" name="payoutOperateTime" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-3 control-label">创建人：</label>
								<div class="col-sm-8">
									<input id="payoutCreator" name="payoutCreator" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">
								<label class="col-sm-3 control-label">创建时间：</label>
								<div class="col-sm-8">
									<input id="payoutCreateTime" name="payoutCreateTime" class="form-control" type="text">
								</div>
							</div>-->
							<div class="form-group">
								<label class="col-sm-2 control-label">主送人：</label>
								<div class="col-sm-8">
									<input id="mainPersonId" name="mainPersonId" type="hidden">
									<input id="isMainPerson" name="isMainPerson" type="hidden">
									<div style="float: left;" id="sendPerson"></div>
									<div style="float: left;">
										<button type="button" class="btn btn-danger btn-xs"
											id="sendPersonBtn" var="sendPersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addSendPerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">抄送人：</label>
								<div class="col-sm-8">
									<input id="copyPersonId" name="copyPersonId" type="hidden">
									<input id="isCopyPerson" name="isCopyPerson" type="hidden">
									<div style="float: left;" id="receivePerson"></div>
									<div style="float: left;">
										<button type="button" class="btn btn-danger btn-xs"
											id="receivePersonBtn" var="receivePersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addRecivePerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-4 col-sm-offset-8">
									<button type="submit" class="btn btn-primary">提交</button>
									<button type="button" class="btn btn-danger" onclick="closeWin()">取消</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/contract/payout/edit.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/payout/relateTravel.js"></script>
	<script type="text/javascript" src="/js/common.js"></script>
	<script type="text/javascript"
		src="/js/appjs/common/mainCopyPerson/mainPersonAndCopyPerson.js"></script>
</body>
</html>
