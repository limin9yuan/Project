<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header">
<link href="/css/layui.css" rel="stylesheet">
</head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<!-- tab标签 -->
						<ul id="myTab" class="nav nav-tabs">
							<li class="active"><a href="#basic" data-toggle="tab">基本信息</a></li>
							<li><a href="#payment" data-toggle="tab">付款信息</a></li>
							<li><a href="#Receivables" data-toggle="tab">收款信息</a></li>
							<li><a href="#plan" data-toggle="tab">计划信息</a></li>
						</ul>
						<!-- 基本信息 -->
						<form class="form-horizontal m-t" id="signupForm">
						<input id="contractId" name="contractId" class="form-control" type="hidden" th:value="${contractId}">
							<div id="myTabContent" class="tab-content">
								<div class="tab-pane fade in active" id="basic">
									<div class="form-group">
										<label class="col-sm-2 control-label">客户名称：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="customerId"
												id="customerId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">业务名称：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="businessId"
												id="businessId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">项目名称：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="projectId"
												id="projectId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">申请人姓名：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractApplicantName"
												id="contractApplicantName" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">合同名称：<span class="mustin">*</label>
										<div class="col-sm-4">
											<input disabled="disabled" id="contractName" name="contractName"
												class="form-control" type="text">
										</div>
										<label class="col-sm-2 control-label">建设单位：<span class="mustin">*</label>
										<div class="col-sm-4">
											<input disabled="disabled" id="contractBuildCompany" name="contractBuildCompany"
												class="form-control" type="text">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">合同类型：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractType"
												id="contractType" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">合同种类：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractCategory"
												id="contractCategory" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">合同总金额：<span class="mustin">*</label>
										<div class="col-sm-4">
											<input disabled="disabled" id="contractTotalPrice" name="contractTotalPrice"
												class="form-control" type="text">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">业务发起人：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractApplicant"
												id="contractApplicant" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">合同拟定人：<span class="mustin">*</label>
										<div class="col-sm-4">
											<input disabled="disabled" id="contractDraftPerson" name="contractDraftPerson"
												class="form-control" type="text">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">销售负责人：<span class="mustin">*</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractSales"
												id="contractSales" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">提交评审时间：<span class="mustin">*</label>
										<div class="col-sm-4">
											<div class="input-group date" id="contractCommitTime">
												<input disabled="disabled" class="form-control" type="text"
													name="contractCommitTime"> <span
													class="input-group-addon"> <span
													class="glyphicon glyphicon-calendar"></span>
												</span>
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">附件：<span class="mustin">*</label>
										<div class="col-sm-4">
											<div class="file-manager">
												<button disabled="disabled" type="button" class="layui-btn" id="test1"
													style="width: 100%;">
													<i class="fa fa-cloud"></i>上传文件
												</button>
												<input disabled="disabled" id="contractAttachment" name="contractAttachment"
													type="hidden">
											</div>
										</div>
										<label class="col-sm-2 control-label">关联合同名称：</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractRelatedId"
												id="contractRelatedId" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">发票类型：</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractInvoiceType"
												id="contractInvoiceType" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">预计开具发票时间：</label>
										<div class="col-sm-4">
											<div class="input-group date" id="contractInvoiceTime">
												<input disabled="disabled" class="form-control" type="text"
													name="contractInvoiceTime"> <span
													class="input-group-addon"> <span
													class="glyphicon glyphicon-calendar"></span>
												</span>
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">硬件设备明细表：</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractHardwareList"
												id="contractHardwareList" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
										<label class="col-sm-2 control-label">软件功能列表：</label>
										<div class="col-sm-4">
											<select disabled="disabled" data-placeholder="请选择" name="contractSoftwareList"
												id="contractSoftwareList" class="form-control chosen-select"
												tabindex="2" style="width: 100%">
												<option value="">请选择</option>
											</select>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-2 control-label">备注：</label>
										<div class="col-sm-10">
											<textarea disabled="disabled" id="contractRemarks" name="contractRemarks"
												class="form-control" rows="3"></textarea>
										</div>
									</div>
								</div>
<!-- 付款信息 -->
								<div class="tab-pane fade" id="payment">
									<div class="ibox">
										<div class="ibox-body">
											<table id="payableTable" data-mobile-responsive="true"> </table>
										</div>
									</div>
								</div>
<!-- 收款信息 -->
								<div class="tab-pane fade" id="Receivables">
									<div class="ibox">
										<div class="ibox-body">
											<table id="receivableTable" data-mobile-responsive="true"></table>
										</div>
									</div>
								</div>
<!-- 计划信息 -->
								<div class="tab-pane fade" id="plan">
									<div class="ibox">
										<div class="ibox-body">
											<table id="planTable" data-mobile-responsive="true"></table>
										</div>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">主送人：</label>
								<div class="col-sm-8">
									<input id="mainPersonId" name="mainPersonId" type="hidden">
									<input id="isMainPerson" name="isMainPerson" type="hidden">
									<div style="float: left;" id="sendPerson"></div>
									<div style="float: left;">
										<button disabled="disabled" type="button" class="btn btn-danger btn-xs"
											id="sendPersonBtn" var="sendPersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addSendPerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">抄送人：</label>
								<div class="col-sm-8">
									<input id="copyPersonId" name="copyPersonId" type="hidden">
									<input id="isCopyPerson" name="isCopyPerson" type="hidden">
									<div style="float: left;" id="receivePerson"></div>
									<div style="float: left;">
										<button disabled="disabled" type="button" class="btn btn-danger btn-xs"
											id="receivePersonBtn" var="receivePersonBtn"
											style="border-radius: 100%; margin-top: 5px; margin-left: 10px"
											onclick="addRecivePerson()">
											<span class="glyphicon glyphicon-plus"></span>
										</button>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-4 col-sm-offset-8">
									<button type="button" id="lastBtn" disabled class="btn btn-primary" onclick="lastStep('myTab',4,'lastBtn','nextBtn')">上一页</button>
									&nbsp;
									<button type="button" id="nextBtn" class="btn btn-primary" onclick="nextStep('myTab',4,'lastBtn','nextBtn')">下一页</button>
									&nbsp;
									<button type="button" class="btn btn-primary" onclick="closeWin()">取消</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script src="/js/layui.js"></script>
	<script src="/js/vue.min.js"></script>
	<script src="/js/plugins/layer/layer.min.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/contract/view.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/contract/Payment_information.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/contract/Receivables_information.js"></script>
	<script type="text/javascript" src="/js/appjs/contract/contract/Planning_information.js"></script>
	<script type="text/javascript" src="/js/common.js"></script>
	<script type="text/javascript"
		src="/js/appjs/common/mainCopyPerson/mainPersonAndCopyPerson.js"></script>
</body>
</html>
