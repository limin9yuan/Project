<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="Purchase_ID" name="Purchase_ID" th:value="${purchaseDetail.Purchase_ID}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">物品名称：</label>
								<div class="col-sm-8">
									<input id="purchaseName" name="purchaseName" th:value="${purchaseDetail.purchaseName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">规格配置：</label>
								<div class="col-sm-8">
									<input id="purchaseConfig" name="purchaseConfig" th:value="${purchaseDetail.purchaseConfig}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">品牌：</label>
								<div class="col-sm-8">
									<input id="purchaseBrand" name="purchaseBrand" th:value="${purchaseDetail.purchaseBrand}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">型号：</label>
								<div class="col-sm-8">
									<input id="purchaseMode" name="purchaseMode" th:value="${purchaseDetail.purchaseMode}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">单位：</label>
								<div class="col-sm-8">
									<input id="purchaseUnit" name="purchaseUnit" th:value="${purchaseDetail.purchaseUnit}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">数量：</label>
								<div class="col-sm-8">
									<input id="purchaseNumber" name="purchaseNumber" th:value="${purchaseDetail.purchaseNumber}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">单价：</label>
								<div class="col-sm-8">
									<input id="purchaseUnitPrice" name="purchaseUnitPrice" th:value="${purchaseDetail.purchaseUnitPrice}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">总价：</label>
								<div class="col-sm-8">
									<input id="purchaseTotalPrice" name="purchaseTotalPrice" th:value="${purchaseDetail.purchaseTotalPrice}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/system/purchaseDetail/edit.js">
	</script>
</body>
</html>
