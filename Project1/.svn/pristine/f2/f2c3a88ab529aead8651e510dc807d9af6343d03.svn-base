	<delete id="remove">
		delete from timesheet where TimeSheet_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from timesheet where TimeSheet_ID in 
		<foreach item="timesheetId" collection="array" open="(" separator="," close=")">
			#{timesheetId}
		</foreach>
	</delete>

