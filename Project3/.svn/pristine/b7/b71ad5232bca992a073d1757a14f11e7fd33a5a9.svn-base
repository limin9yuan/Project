	<parameter name="t4" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select  stat_id,stat_name,source_id,source_name,
count(distinct house_code) house_count,use_type,
sum(use_area) use_area,
sum(build_area) build_area,
sum(super_area) super_area,
sum(charge_area) charge_area,
sum(A1_barea) A1_barea,
sum(A2_barea) A2_barea,
sum(A3_barea) A3_barea,
sum(A4_barea) A4_barea,
sum(A1_area) A1_area,
sum(A2_area) A2_area,
sum(A3_area) A3_area,
sum(A4_area) A4_area,
sum(charge_area) charge_area
from (
		select a.community_code,a.community_name,a.building_code,a.building_name,a.house_code,
		b.use_area,b.build_area,b.super_area,b.charge_area,b.use_type,
		case when b.standard_id='$P!{item_code}1' then b.build_area else 0 end A1_barea,
                case when b.standard_id='$P!{item_code}2' then b.build_area else 0 end A2_barea,
                case when b.standard_id='$P!{item_code}3' then b.build_area else 0 end A3_barea,
		case when b.standard_id='$P!{item_code}4' then b.build_area else 0 end A4_barea,
		case when b.standard_id='$P!{item_code}1' then b.charge_area else 0 end A1_area,
                case when b.standard_id='$P!{item_code}2' then b.charge_area else 0 end A2_area,
                case when b.standard_id='$P!{item_code}3' then b.charge_area else 0 end A3_area,
		case when b.standard_id='$P!{item_code}4' then b.charge_area else 0 end A4_area,
		b.standard_id ,
                a.stat_id,d.stat_name,e.source_id,e.source_name
		from v_house a,fc_area b,sf_standard c,fc_station d,fc_source e,sf_item f
		where a.house_code=b.house_code
		and b.standard_id=c.standard_id(+)
                and a.stat_id=d.stat_id(+)
                and d.source_id=e.source_id(+)
                and c.item_code=f.item_code(+)
                and c.standyear=f.current_year(+)
                and f.item_code='A'
		) a where 1=1 
		and ('$P!{is_null}'='1' or (community_code in ($P!{p_range}) or 
building_code in ($P!{p_range}) or house_code in ($P!{p_range})))
		and (('$P!{standard_id}'='-1' and standard_id is null ) 
		or '$P!{standard_id}'='1'
		or standard_id='$P!{standard_id}')		
		group by stat_id,stat_name,source_id,source_name,use_type
		order by stat_id,use_type  asc]]></queryString>

