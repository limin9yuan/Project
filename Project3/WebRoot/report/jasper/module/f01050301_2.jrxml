	<parameter name="queryDate" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select 
sum(case when standard_id='B1' then intoweb_area else 0 end) B1_area,
sum(case when standard_id='B2' then intoweb_area else 0 end) B2_area,
sum(case when standard_id='B3' then intoweb_area else 0 end) B3_area,
sum(intoweb_area) intoweb_area,
sum(case when standard_id='B1' then intoweb_money else 0 end) B1_money,
sum(case when standard_id='B2' then intoweb_money else 0 end) B2_money,
sum(case when standard_id='B3' then intoweb_money else 0 end) B3_money,
sum(intoweb_money) intoweb_money,
stat_id,stat_name,b.source_id,b.source_name,
count(distinct web_code) web_count
from 
		(select 
		a.i_id, 
		a.web_code,
	    a.WEB_NAME,
	    a.WEB_TYPE,
		a.CID, 
		c.PRICE,
		a.INTOWEB_DATE, 
		a.INTOWEB_AREA, 
	    a.STANDARD_ID, 
	    a.OPERATOR, 
		a.OPERATE_DATE,
		b.userName,
		c.standard_name,
		a.INTOWEB_YEAR,
		f.real_account INTOWEB_MONEY,
		a.remark,d.stat_id,d.stat_name,d.source_id,f.community_code,f.building_code,f.house_code
		from IW_INTOWEB a,EAUSER b,sf_standard c ,fc_station d,fc_house e,sf_charge f
		where a.OPERATOR=b.USERID(+)
		and a.STANDARD_ID=c.STANDARD_ID(+)
                and a.web_type='用户'
                and a.web_code=e.house_code(+)
                and e.stat_id=d.stat_id(+)
	and a.web_code=f.house_code(+)
        and f.item_code(+)='B'
        and a.area_id=f.meter_id(+)
        and a.intoweb_year=f.charge_month(+)
                union all
                select 
		a.i_id, 
		a.web_code,
	    a.WEB_NAME,
	    a.WEB_TYPE,
		a.CID, 
		c.PRICE,
		a.INTOWEB_DATE, 
		a.INTOWEB_AREA, 
	    a.STANDARD_ID, 
	    a.OPERATOR, 
		a.OPERATE_DATE,
		b.userName,
		c.standard_name,
		a.INTOWEB_YEAR,
		f.real_account INTOWEB_MONEY,
		a.remark,d.stat_id,d.stat_name,d.source_id,f.community_code,f.building_code,f.house_code
		from IW_INTOWEB a,EAUSER b,sf_standard c ,fc_station d,fc_community e,sf_charge f
		where a.OPERATOR=b.USERID(+)
		and a.STANDARD_ID=c.STANDARD_ID(+)
                and a.web_type='小区'
                and a.web_code=e.community_code(+)
                and e.stat_id=d.stat_id(+)
	and a.web_code=f.house_code(+)
        and f.item_code(+)='B'
        and a.area_id=f.meter_id(+)
        and a.intoweb_year=f.charge_month(+)
                union all
                select 
		a.i_id, 
		a.web_code,
	    a.WEB_NAME,
	    a.WEB_TYPE,
		a.CID, 
		c.PRICE,
		a.INTOWEB_DATE, 
		a.INTOWEB_AREA, 
	    a.STANDARD_ID, 
	    a.OPERATOR, 
		a.OPERATE_DATE,
		b.userName,
		c.standard_name,
		a.INTOWEB_YEAR,
		f.real_account INTOWEB_MONEY,
		a.remark,d.stat_id,d.stat_name,d.source_id,f.community_code,f.building_code,f.house_code
		from IW_INTOWEB a,EAUSER b,sf_standard c ,fc_station d,fc_building e,sf_charge f
		where a.OPERATOR=b.USERID(+)
		and a.STANDARD_ID=c.STANDARD_ID(+)
                and a.web_type='大楼'
                and a.web_code=e.building_code(+)
                and e.stat_id=d.stat_id(+)
		and a.STANDARD_ID=c.STANDARD_ID(+)
	and a.web_code=f.house_code(+)
        and f.item_code(+)='B'
        and a.area_id=f.meter_id(+)
        and a.intoweb_year=f.charge_month(+)
		) a ,fc_source b
where a.source_id=b.source_id 
and ('$P!{is_null}'='1' or (COMMUNITY_CODE in ($P!{p_range}) or BUILDING_CODE in ($P!{p_range})    or COMMUNITY_CODE in ($P!{p_range})
                ))
		and (('$P!{standard_id}'='-1' and standard_id is null ) 
		or '$P!{standard_id}'='1'
		or standard_id='$P!{standard_id}')
and ($P!{charge_month}='-1' or INTOWEB_YEAR in($P!{charge_month}))
$P!{queryDate}
group by stat_id,stat_name,b.source_id,b.source_name]]></queryString>

